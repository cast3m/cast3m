C SAXPY     SOURCE    CHAT      05/01/13    03:12:00     5004
      SUBROUTINE SAXPY(N,SA,SX,INCX,SY,INCY)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
      REAL*8 SA,SX(1),SY(1)
C
C         CE SOUS PROGRAMME FAIT PARTIE DE LA BIBLIOTHEQUE CRAY
C           ET DOIT ETRE CHARGE UNIQUEMENT SUR IBM
C
C
      Z=0.D0
      DO 1 I=1,N
      SY(I)=SY(I)+SA*SX(I)
 1    CONTINUE
      RETURN
      END



