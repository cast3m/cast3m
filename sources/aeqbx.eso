C AEQBX     SOURCE    CHAT      05/01/12    21:19:13     5004
      SUBROUTINE AEQBX (A,B,X,LA)
C
C CALCULE A=B*X
C
C A ET B TABLEAUX DE LONGUEUR LA
C X REEL
C
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8(A-H,O-Z)
C
      DIMENSION A(*),B(*)
C
      DO 1 IA=1,LA
 1    A(IA)=X*B(IA)
      RETURN
      END

