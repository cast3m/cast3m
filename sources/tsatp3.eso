C TSATP3    SOURCE    CHAT      05/01/13    03:53:16     5004
      SUBROUTINE TSATP3(MLREE1,MLREE3)
C--------------------------------------------------------------------
C Température de saturation de la vapeur pour une pression partielle
C de vapeur donnée.
C--------------------------------------------------------------------
C
C  E/   MLREE1 : Pression partielle de vapeur (en Pa)
C   /S  MLREE3 : Température de saturation (en K)
C
C--------------------------------------------------------------------
C
C Langage : ESOPE + FORTRAN77
C Auteurs : F.DABBENE 01/00
C
C-----------------------------------------------------------------------
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
C
-INC SMLREEL
C
      JG1 = MLREE1.PROG(/1)
      DO 10 I=1,JG1
         X1 = MLREE1.PROG(I)
         X3 = TSATP0(X1)
         MLREE3.PROG(I) = X3
 10   CONTINUE
C
      RETURN
      END


