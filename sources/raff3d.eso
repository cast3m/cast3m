c                NEXIS2=0
c                NMIN2=MIN(NMAX,NMILI)
c                NMAX2=MAX(NMAX,NMILI)
c                DO 319 I2=1,KMILIE(/2)
c                  IF (KMILIE(NMIN2,I2).GT.0) THEN
