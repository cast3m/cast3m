c1/ Test sur la premiere arete : NMIN-KMILIE(NMIN,I)
c                NMILI = ICPR(KMILIE(NMIN,I),1)
c                NEXIS2=0
c                NMIN2=MIN(NMIN,NMILI)
c                NMAX2=MAX(NMIN,NMILI)
c                DO 318 I2=1,KMILIE(/2)
