C SJACO7    SOURCE    PV        15/10/08    21:15:04     8665
*****************************************************
*
* affecte 1 à T si la convergence est réalisee
*
*****************************************************
C
          SUBROUTINE SJACO7(IPK1,T,N,K)
C
      IMPLICIT INTEGER(I-N)
          SEGMENT ,MATRIX
           REAL*8 A(N,N)
          ENDSEGMENT
C
          POINTEUR IPK1.MATRIX
          REAL*8 F,S
C
          INTEGER N,T,I,J
C
           SEGACT ,IPK1
C
          T=1
          S=(1.D-12)
C
          DO 100 J=1,N
C
             DO 150 I=(J+1),N
               F=SQRT((IPK1.A(I,J)**2)/(IPK1.A(I,I)*IPK1.A(J,J)))
               IF (F.GT.S) THEN
                 T=0
                 GOTO 10
               ENDIF
150          CONTINUE
C
100       CONTINUE
C
10      continue
*        SEGDES ,IPK1
          RETURN
          END



