C PISCAL    SOURCE    CHAT      05/01/13    02:14:15     5004
      FUNCTION PISCAL (A,B,NL)
C
C-----------------------------------------------------------
C PRODUIT SCALAIRE DES VECTEURS A B DE LONGUEUR NL
C-----------------------------------------------------------
C
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
C
      DIMENSION A(1),B(1)
C
      PIS=0.
      DO 10 I=1,NL
      PIS=PIS+A(I)*B(I)
   10 CONTINUE
C
      PISCAL=PIS
C
      RETURN
      END

