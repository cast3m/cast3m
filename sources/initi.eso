C INITI     SOURCE    CHAT      05/01/13    00:38:14     5004
      SUBROUTINE INITI(IA,LONG,IVAL)
C************************************************************************
C
C     Remplit le tableau d'entiers IA(LONG) avec IVAL
C
C************************************************************************
      IMPLICIT INTEGER(I-N)
      INTEGER IA(LONG)

      DO 1 I=1,LONG
         IA(I)=IVAL
 1    CONTINUE
      RETURN
      END



