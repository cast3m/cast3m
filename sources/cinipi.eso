      subroutine cinipi(jcolac)
C Sous programme mini (MPI INItialisation des PIles)
C Remet a zero les piles d'entiers, flottant, mot et logique
      integer iPile
      integer ks
-INC TMCOLAC
      pointeur jcolac.ICOLAC
      pointeur pile.ITLACC
C      write(6,*) 'Entree dans CINIPI'
        ks=0
        do iPile=24,27
          segini pile
          jcolac.kcola(iPile)=pile
        enddo
      end



