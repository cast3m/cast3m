C FONZ      SOURCE    CHAT      05/01/13    00:06:27     5004
      FUNCTION FONZ(T)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
      COMMON/CMCHAM/ D1,D2,R,A,U,DD1,DD2,LSOMD,LD1,LD2
      COMMON/CMARGT/SINT,COST,RSINT,RCOST,RSIN2,XA,XU,XXA,XXU,SQA2,SQA1
     +,SQU2,SQU1
      LOGICAL LSOMD,LD1,LD2
C     FUNCTION FZ1
      FZ1(D,XA,XU,SQA,SQU)=D* LOG((XA+SQA)/(XU+SQU))
      CALL ARGUME(T)
      IF(LSOMD) GOTO 3
4     IF(LD1)   GOTO 6
7     IF(LD2)   GOTO 8
      GOTO 9
3     F1= FZ1(D2,XA,XU,SQA2,SQU2)*2.D0
      F2= FZ2(D2, A, U,SQA2,SQU2,RSIN2)*2.D0
      F3= FZ3(D2,XA,XU,SQA2,SQU2,RSINT)*2.D0
      GOTO 10
6     F1= FZ1(D2,XA,XU,SQA2,SQU2)
      F2= FZ2(D2, A, U,SQA2,SQU2,RSIN2)
      F3= FZ3(D2,XA,XU,SQA2,SQU2,RSINT)
      GOTO 10
8     F1=-FZ1(D1,XA,XU,SQA1,SQU1)
      F2=-FZ2(D1, A, U,SQA1,SQU1,RSIN2)
      F3=-FZ3(D1,XA,XU,SQA1,SQU1,RSINT)
      GOTO 10
9     F1= FZ1(D2,XA,XU,SQA2,SQU2)-FZ1(D1,XA,XU,SQA1,SQU1)
      F2= FZ2(D2, A, U,SQA2,SQU2,RSIN2)-FZ2(D1, A, U,SQA1,SQU1,RSIN2)
      F3= FZ3(D2,XA,XU,SQA2,SQU2,RSINT)-FZ3(D1,XA,XU,SQA1,SQU1,RSINT)
10    FONZ=F1+F2+F3
      RETURN
      END

