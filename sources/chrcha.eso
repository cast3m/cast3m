C CHRCHA    SOURCE    CHAT      05/01/12    22:01:43     5004
      SUBROUTINE CHRCHA(MOT,ITYP,IRET,ISOLIT)
C====================================================================
C     SUBROUTINE FORTRAN POUR CHERCHER DANS LE MSOLUT DE TYPE ITYP
C     LE RANG IRET DES CHAMPS DE TYPE MOT.
C     ECRIT PAR FARVACQUE 16/10/85
C     IS....= 2 : CHPOINT   |
C     IS....= 5 : MCHELM    |   POUR MSOLIT(IRET). VOIR LE SOUS PROG.
C     IS....= 14: MJONCT    |   TYPFIL QUI MET EN CORRSPONDANCE LE TYPE
C                               ET LE NUMERO
C======================================================================
      IMPLICIT INTEGER(I-N)
      CHARACTER*4 MOT
      CHARACTER*8 ITYP
      CHARACTER*4 IDYNA(9),IMODE(5),ISOLS(6),IGREEN(1)
      DIMENSION ISDYNA(9),ISMODE(5),ISSOLS(6),ISGREE(1)
      DATA LDYNA/9/,LMODE/5/,LSOLS/6/,LGREE/1/
      DATA IMODE/'DEPL','CONT','VMIS','VITE','ACCE'/
      DATA ISMODE/2,5,5,2,2/
      DATA ISOLS/'DEPL','CONT','VMIS','VITE','ACCE','JONC'/
      DATA ISSOLS/2,5,5,2,2,14/
C
C  2 MOTS SONT BLANCS DS IDYNA CORRESPONDANT AUX MSOLIS(11 & 12) RESERVE
C  PAR ALGOGD . DEVO UTILSE MSOLIS(13)
C
      DATA IDYNA/'DEPL','CONT','VMIS','VITE','ACCE','LIAI','    ',
     &           '    ','USUR'/
      DATA ISDYNA/2,5,5,2,2,2,0,0,2/
      DATA IGREEN/'GREE'/
      DATA ISGREE/2/
C
      IRET=0
      ISOLIT=0
      IF(ITYP.NE.'MODE    ') GOTO 100
      CALL CHRMOT(IMODE,LMODE,MOT,IRET)
      IF(IRET.NE.0) THEN
        ISOLIT=ISMODE(IRET)
        IRET=IRET+4
      ENDIF
      GOTO 5000
  100 CONTINUE
      IF(ITYP.NE.'SOLUSTAT'.AND.ITYP.NE.'PSEUMODE') GOTO 101
      CALL CHRMOT(ISOLS,LSOLS,MOT,IRET)
      IF(IRET.NE.0) THEN
        ISOLIT=ISSOLS(IRET)
        IRET=IRET+4
      ENDIF
      GOTO 5000
  101 CONTINUE
      IF(ITYP.NE.'DYNAMIQU')GOTO 102
      CALL CHRMOT(IDYNA,LDYNA,MOT,IRET)
      IF(IRET.NE.0) THEN
        ISOLIT=ISDYNA(IRET)
        IRET=IRET+4
      ENDIF
      GOTO 5000
  102 CONTINUE
      IF(ITYP.NE.'FONCGREE')GOTO 103
      CALL CHRMOT(IGREEN,LGREE,MOT,IRET)
      IF(IRET.NE.0) THEN
        ISOLIT=ISGREE(IRET)
        IRET=IRET+4
      ENDIF
      GOTO 5000
  103 CONTINUE
 5000 CONTINUE
      RETURN
      END

