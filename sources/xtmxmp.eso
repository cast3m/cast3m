C XTMXMP    SOURCE    CHAT      05/01/13    04:15:07     5004
      SUBROUTINE XTMXMP(NQ,CC,ICPR,NUM,I2,NOELEM,IPOS,VAA,NNIN,N1)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8(A-H,O-Z)
      DIMENSION CC(*),ICPR(*),NUM(N1,*),NOELEM(*),IPOS(*),VAA(NNIN,*)
      DO 34 IN=1,NQ
  34  CC(IN)=0.D0
      DO 33 IN=1,NQ
      J2=ICPR(NUM(NOELEM(IN),I2))
      IF(J2.EQ.0) GO TO 33
      J1=IPOS(IN)
      CC(IN)=VAA(J1,J2)
   33 CONTINUE
C
      RETURN
      END

