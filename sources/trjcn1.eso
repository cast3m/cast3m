C TRJCN1    SOURCE    BP208322  16/11/18    21:21:47     9177
      SUBROUTINE TRJCN1(MELEME)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C
C         CONTROLE QUE LE MAILLAGE MELEME CONVIENT AU
C                 CALCUL DES TRAJECTOIRES
C     POUR RAJOUTER UN TYPE D ELEMENT DONT LE NOMBRE DE NOEUD SERA
C      SUPERIEUR A 9 IL FAUDRA MODIFIER MNO9 DANS TRJPAR
C      ENTREES
C          MELEME POINTEUR DU MAILLAGE
C
CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
-INC SMELEME
-INC CCOPTIO
-INC CCGEOME
      PARAMETER (NDTYP=8)
      DIMENSION IDTYP(NDTYP)
      DATA IDTYP/4,6,7,8,11,14,16,23/
C
      SEGACT MELEME
      NBSOUS=LISOUS(/1)
      NBS=NBSOUS
      IF(NBSOUS.EQ.0)NBS=1
      IPT1=MELEME
      DO 50 ISOUS=1,NBS
      IF(NBSOUS.GT.0)IPT1=LISOUS(ISOUS)
      SEGACT IPT1
      ITP=IPT1.ITYPEL
      DO 20 I=1,NDTYP
      IF(ITP.EQ.IDTYP(I))GO TO 25
   20 CONTINUE
      MOTERR(1:4)=NOMS(ITP)(1:4)
      MOTERR(5:12)='TRJCN1  '
      CALL ERREUR(86)
      RETURN
   25 CONTINUE
       SEGDES IPT1
   50 CONTINUE
C
      IF(NBSOUS.NE.0)SEGDES MELEME
      RETURN
      END










