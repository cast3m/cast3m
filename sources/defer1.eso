      SUBROUTINE DEFER1(JNOID,KERR1,KERRE,LOGSUC)
*
***************************************************
*  ENTREES
***************************************************
*
* JNOID  : test les problèmes de convergence
* KERR1  : indice d'erreur
* KERRE  : indice d'erreur
*
***************************************************
*  SORTIES
***************************************************
*
* KERR1  : indice d'erreur
* KERRE  : indice d'erreur
* LOGSUC : booléen d'erreur
*
***************************************************
*
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8(A-H,O-Z)
*
      LOGICAL LOGSUC
*
*  mill 3/2/93    test sur jnoid si pb de non convergence
*
          IF(JNOID.EQ.1) THEN
             IF(KERR1.EQ.1) THEN
               IF(KERRE.EQ.460.OR.KERRE.EQ.465.OR.KERRE.EQ.466) THEN
                  KERR1=0
c                  KERRE=0
                  LOGSUC=.FALSE.
               ENDIF
             ENDIF
             IF(KERRE.EQ.2) THEN
               KERRE=0
               LOGSUC=.FALSE.
             ENDIF
          ENDIF
c
      RETURN
      END



