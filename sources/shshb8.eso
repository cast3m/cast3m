      subroutine shshb8 ( qsi, shp)
      implicit real*8 (a-h,o-z)
      implicit integer (i-n)
      dimension shp(6,8)
      shp(1,1)=(1.d0 -qsi)/8.d0
      shp(1,2)=(1.d0 -qsi)/8.d0
      shp(1,3)=(1.d0 -qsi)/8.d0
      shp(1,4)=(1.d0 -qsi)/8.d0
      shp(1,5)=(1.d0+ qsi ) / 8.d0
      shp(1,6)=(1.d0+ qsi ) / 8.d0
      shp(1,7)=(1.d0+ qsi ) / 8.d0
      shp(1,8)=(1.d0+ qsi ) / 8.d0

      return
      end

