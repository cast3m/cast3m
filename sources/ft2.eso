C FT2       SOURCE    CHAT      05/01/13    00:10:58     5004
      FUNCTION FT2(D,SQA,SQU,LD)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
      COMMON/CMCHAM/ D1,D2,R,A,U,DD1,DD2,LSOMD,LD1,LD2
      COMMON/CMARGT/SINT,COST,RSINT,RCOST,RSIN2,XA,XU,XXA,XXU,SQA2,SQA1
     +,SQU2,SQU1
      LOGICAL LSOMD,LD1,LD2,LD
      IF(.NOT.LD)    GOTO 4
3     IF(COST.EQ.1.D0) GOTO 5
4     FT2=-R*SINT*COST*LOG((XA+SQA)/(XU+SQU))
      RETURN
5     FT2=0.D0
      RETURN
      END

