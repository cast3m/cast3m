        IF  (ICLE.EQ.3) THEN
        CALL TRMESS ('Cliquer sur la colonne.')
        CALL TBGTXY (CURPX,CURPY,TABTR,IX,IY,FX,FY)
        IF  (IX.EQ.0) IX=TABTR.CIDX (CURPX,CURPY)+1
        DO 686 IY=1 , ITABY
          TABTR.ZGVSEP (IX,IY) = .FALSE.
          TABTR.ZVSEP   (IX,IY) = .FALSE.
686     CONTINUE
        ENDIF
