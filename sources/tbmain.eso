*
710     CONTINUE
        CALL TBAFF  ( CURPX, CURPY, TABTR )
        LEGEND (1)='<< options-divers'
        IF  (TABTR.ZDATE) THEN
          LEGEND (2)='     (X)  date'
        ELSE
          LEGEND (2)='     ( )  date'
        ENDIF
        IF  (TABTR.ZCTRER) THEN
          LEGEND (3)='   (X) centrer'
        ELSE
          LEGEND (3)='   ( ) centrer'
        ENDIF
        IF  (TABTR.ZAULIG) THEN
          LEGEND (4)='    (X) lignes'
        ELSE
          LEGEND (4)='    ( ) lignes'
        ENDIF
        IF  (TABTR.ZPAGE) THEN
          LEGEND (5)='   (X) No page'
        ELSE
          LEGEND (5)='   ( ) No page'
        ENDIF
          LEGEND (6)='     Logo >> '
        CALL MENU (LEGEND,6,17)
        CALL TRAFF (ICLE)
*  REVENIR
        IF  (ICLE.EQ.0) GOTO 610
*  DATE
        IF  (ICLE.EQ.1) THEN
          TABTR.ZDATE = .NOT.TABTR.ZDATE
          GOTO 710
        ENDIF
*  CENTRER
        IF  (ICLE.EQ.2) THEN
          TABTR.ZCTRER = .NOT.TABTR.ZCTRER
        CALL TBTRXY (TABTR,NBPX,NBPY)
          GOTO 710
        ENDIF
*  AUTOLIGNES
        IF  (ICLE.EQ.3) THEN
          TABTR.ZAULIG = .NOT.TABTR.ZAULIG
          GOTO 710
        ENDIF
*  PAGES
        IF  (ICLE.EQ.4) THEN
          TABTR.ZPAGE = .NOT.TABTR.ZPAGE
          GOTO 710
        ENDIF
*  LOGO
        IF  (ICLE.EQ.5) THEN
          GOTO 720
        ENDIF
*  RETOUR
        GOTO 710
*
*  SOUS MENU LOGO
*
720     CONTINUE
        CALL TBAFF  ( CURPX, CURPY, TABTR )
        LEGEND (1)=' << logo'
        LEGEND (2)='Position'
        LEGEND (3)='Couleur'
        LEGEND (4)='Taille'
        IF  (TABTR.ZLOGO) THEN
          LEGEND (5)=' (X) Logo'
        ELSE
          LEGEND (5)=' ( ) Logo'
        ENDIF
        CALL MENU (LEGEND,5,9)
