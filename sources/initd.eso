      SUBROUTINE INITD(A,LONG,VAL)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
C************************************************************************
C
C     SOUS PROGRAMME DOUBLE PRECISION
C
C************************************************************************
      REAL*8 A,VAL
      DIMENSION A(1)
      DO 1 I=1,LONG
      A(I)=VAL
 1    CONTINUE
      RETURN
      END

