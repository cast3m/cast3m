C TEQUIV    SOURCE    CHAT      05/01/13    03:36:10     5004
       SUBROUTINE TEQUIV(K1,RO1,T0,FP,K2,SIGP,DSIGP,TEQ)
      IMPLICIT INTEGER(I-N)
       IMPLICIT REAL*8 (A-H,O-Z)
       REAL*8 K1,K2,MU
       MU=(SIGP+DSIGP)/FP
       EXPOS1=(4.D0/3.D0)/(1.D0-MU)
       EXP2=EXP((10.D0*MU-7.5D0)/K2)
       TEQ=T0+(1.D3/24.D0)*((DSIGP/(K1*RO1*
     &                      (SIGP+DSIGP)*EXP2))**EXPOS1)
       RETURN
       END


