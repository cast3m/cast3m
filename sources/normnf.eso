CCC
C Norma infinito de un vector
CCC
             FUNCTION NORMAINF (V,N)
      IMPLICIT INTEGER(I-N)
      integer i,n
      real*8    v(n),aux, normainf
      aux=ABS(v(1))
      do i=2,n
         if (abs(v(i)).gt.aux) aux=abs(v(i))
      enddo
      normainf=aux
      return
      end





