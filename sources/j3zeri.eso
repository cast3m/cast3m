      SUBROUTINE J3ZERI(IJ,NI,NJ)
C-----------------------------------------------------
C     ON MET A ZERO LES ELEMENTS DE IJ
C
C     PP 6/97
C     Pierre Pegon/JRC Ispra
C-----------------------------------------------------
C
      IMPLICIT INTEGER(I-N)
      DIMENSION IJ(NI,NJ)
C
      DO J=1,NJ
        DO I=1,NI
          IJ(I,J)=0
        ENDDO
      ENDDO
C
      RETURN
      END


