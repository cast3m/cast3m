C COPIVE    SOURCE    CHAT      05/01/12    22:20:24     5004
      SUBROUTINE COPIVE(V1,IDIMC,V2)
C     ***************************************************************
C     COPIE UN VECTEUR DANS UN AUTRE : V2 <- V1
C     ***************************************************************
      IMPLICIT INTEGER(I-N)
      INTEGER IDIMC
      REAL*8    V1(*),V2(*)
C
      INTEGER I
C
      IF(IDIMC.LE.0) RETURN
      DO 10 I=1,IDIMC
        V2(I) = V1(I)
   10 CONTINUE
      END


