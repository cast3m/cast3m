C CALG      SOURCE    CHAT      05/01/12    21:46:09     5004
       FUNCTION CALG(SIG1,SIG2,SIGNE,PHI,C)
      IMPLICIT INTEGER(I-N)
       IMPLICIT REAL*8(A-H,O-Z)
C
C----------CALCUL DU CRITERE DE CISAILLEMENT DE LA LOI DE
C----------MOHR COULOMB POUR LES JOINTS 3D
C
       CALG = 0.0D0
       CALG = (SQRT( (SIG1**2) + (SIG2**2) ))  + (SIGNE * TAN(PHI)) - C
C
       RETURN
       END


