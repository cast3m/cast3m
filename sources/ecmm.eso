C ECMM      SOURCE    CHAT      05/01/12    23:24:18     5004
      SUBROUTINE ECMM(ITABLE,MOTI,MOT1I)
C
C ECRIRE UN MOT DESIGNE PAR UN MOT
C
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8(A-H,O-Z)
      CHARACTER*(*) MOTI,MOT1I
      CHARACTER*8 MOT,MOT1
      MOT=MOTI
      MOT1=MOT1I
      CALL LENCHA(MOT,LC)
      CALL LENCHA(MOT1,LC1)
      CALL ECCTAB(ITABLE,'MOT',0,0.D0,MOT(1:LC), .TRUE.,0,
     1                   'MOT',0,0.D0,MOT1(1:LC1),.TRUE.,0)
      RETURN
      END

