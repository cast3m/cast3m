C MATMAT    SOURCE    CHAT      05/01/13    01:34:43     5004
      SUBROUTINE MATMAT(A,B,NL,NC1,NC2,C)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
*
*  C(NL,NC2)=A(NL,NC1)*B(NC1,NC2)
*  LE PREMIER INDICE EST DE LIGNE, LE DEUXIEME DE COLONNE
*
      DIMENSION A(NL,*),B(NC1,*),C(NL,*)
      DO 1 I=1,NL
      DO 1 J=1,NC2
      XX= 0.D0
      DO 2 K=1,NC1
      XX = A(I,K)*B(K,J) + XX
    2 CONTINUE
      C(I,J)=XX
    1 CONTINUE
      RETURN
      END

