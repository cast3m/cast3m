C MEDTRA    SOURCE    CB215821  19/03/18    21:15:37     10161
      subroutine medtra(meleme,icle)
      implicit real*8(a-h,o-z)
      implicit integer (i-n)

C ----------------------------------------------------------------------
C icle=1 passage de la numerotation Cast3M à MED
C icle=2 passage de la numerotation MED à Cast3M
C
C        creation d'un nouveau MELEME avec les connectivites pour MED 3
C ----------------------------------------------------------------------


-INC CCOPTIO
-INC SMELEME
-INC CCGEOME

C     Définition du tableau des permutations
      dimension iper(80)
      data iper/3,5,2,4,6  ,3,5,7,2,4,6,8,
     & 3,5,7,13,15,17,19,2,4,6,8,14,16,18,20,9,10,11,12,
     & 3,5,10,12,14,2,4,6,11,13,15,7,8,9,
     & 3,5,10,2,4,6,7,8,9,
     & 3,5,2,4,6,7,  3,5,7,2,4,6,8,9,
     & 3,5,7,13,2,4,6,8,9,10,11,12/

C     Cas des éléments de degré différent de 3
      if(kdegre(itypel).ne.3) return

      ipo=-1

      if(itypel.eq.6) then
C       Cas des TRI6
        ipo=0
      elseif(itypel.eq.10) then
C       Cas des QUA8
        ipo=5
      elseif(itypel.eq.15) then
C       Cas des CU20
        ipo=12
      elseif(itypel.eq.17) then
C       Cas des PR15
        ipo=31
      elseif(itypel.eq.24) then
C       Cas des TE10
        ipo=45
      elseif(itypel.eq.7) then
C       Cas des TRI7
        ipo=54
      elseif(itypel.eq.11) then
C       Cas des QUA9
        ipo=60
      elseif(itypel.eq.26) then
C       Cas des PY13
        ipo=68
      endif

C     Certains éléments n'ont pas besoin de modification de connectivité
      if(ipo.eq.-1) return

C     Copie du meleme dans ipt1
      segini,ipt1=meleme

      nn= meleme.num(/1)
      ia= meleme.itypel

      if(icle.eq.1) then
        do ib=1,meleme.num(/2)
          do ia = 1,nn-1
            ipt1.num(ia+1,ib)=meleme.num(iper(ipo+ia),ib)
          enddo
        enddo
        meleme=ipt1

      elseif(icle.eq.2) then
        do ib=1,meleme.num(/2)
          do ia = 1,nn-1
            ipt1.num(iper(ipo+ia),ib)=meleme.num(ia+1,ib)
          enddo
        enddo
        meleme=ipt1
      else
        CALL ERREUR(5)
      endif

      end

