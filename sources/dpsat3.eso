C DPSAT3    SOURCE    CHAT      05/01/12    22:54:57     5004
      SUBROUTINE DPSAT3(MLREE1,MLREE3)
C--------------------------------------------------------------------
C Dérivée de la pression partielle de vapeur à saturation
C--------------------------------------------------------------------
C
C  E/   MLREE1 : Température de saturation (en K)
C   /S  MLREE3 : Dérivée de la pression partielle de vapeur à
C                saturation (en Pa/K)
C
C--------------------------------------------------------------------
C
C Langage : ESOPE + FORTRAN77
C 12/2002
C
C-----------------------------------------------------------------------
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
C
-INC SMLREEL
C
      JG1 = MLREE1.PROG(/1)
      DO 10 I=1,JG1
         X1 = MLREE1.PROG(I)
         X3 = DPSAT0(X1)
         MLREE3.PROG(I) = X3
 10   CONTINUE
C
      RETURN
      END



