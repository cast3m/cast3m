C PSATT3    SOURCE    CHAT      05/01/13    02:37:06     5004
      SUBROUTINE PSATT3(MLREE1,MLREE3)
C--------------------------------------------------------------------
C Pression partielle de vapeur à saturation pour une température donnée
C--------------------------------------------------------------------
C
C  E/   MLREE3 : Température de saturation (en K)
C   /S  MLREE1 : Pression partielle de vapeur à saturation (en Pa)
C
C--------------------------------------------------------------------
C
C Langage : ESOPE + FORTRAN77
C Auteurs : F.DABBENE 01/00
C
C-----------------------------------------------------------------------
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
C
-INC SMLREEL
C
      JG1 = MLREE1.PROG(/1)
      DO 10 I=1,JG1
         X1 = MLREE1.PROG(I)
         X3 = PSATT0(X1)
         MLREE3.PROG(I) = X3
 10   CONTINUE
C
      RETURN
      END


