C UTMXV     SOURCE    CB215821  17/11/30    21:17:24     9639
      SUBROUTINE UTMXV(B,M,BMAX)
C
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
      DIMENSION B(M)
C
C     CALCUL DU MAXIMUM BMAX D'UNE MATRICE B
C
      AB = B(1)
      BMAX = ABS(AB)
      DO 1 I= 1,M
      AB = ABS(B(I))
      BMAX =  MAX(BMAX,AB)
  1   CONTINUE
      RETURN
      END




