C TIFST2    SOURCE    AM        94/02/25    22:58:11     1061
      SUBROUTINE TIFST2(COORD,DEPLA,LHOOK,DDHOOK,CONT,WORK,IDEUX)
C-----------------------------------------------------------------------
C  ROUTINE DE CALCUL DES CONTRAINTES POUR LA POUTRE DROITE
C  DE TIMOSHENKO MULTIFIBRE EN 2D
C
C  ENTREE
C          DEPLA   DEPLACEMENTS
C          DDHOOK(LHOOK,LHOOK) MATRICE DE HOOK
C          COORD   COORDONNEES DES 2 NOEUDS
C          IDEUX   INDICE POUR DEFORMATIONS DU SECOND ORDRE
C
C  SORTIE
C          CONT    CONTRAINTES AU MILIEU
C  TRAVAIL
C          WORK(300)
C
C  D'APRES TIFSTR DC 98
C-----------------------------------------------------------------------
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8(A-H,O-Z)
      DIMENSION CONT(*),COORD(3,*),P(2,2),
     .          XE(2),YE(2),DEPLA(*),DDHOOK(LHOOK,LHOOK)
C
      DO 2 I=1,2
      XE(I)=COORD(1,I)
  2   YE(I)=COORD(2,I)
      XL=(XE(2)-XE(1))**2+(YE(2)-YE(1))**2
      XL=SQRT(XL)
C
C  CALCUL DE LA MATRICE DE PASSAGE
C
      CALL POUPA2(XE,YE,P,KERRE)
C
C  PASSAGE DES DEPLACEMENTS EN AXES LOCAUX
C
      CALL POUVE2(DEPLA,WORK,P,1)
C
C  CALCUL DES CONTRAINTES LOCALES
C
      CALL TIFOL2(XL,WORK,CONT,LHOOK,DDHOOK,IDEUX)
      RETURN
      END


