C DPSAT1    SOURCE    CHAT      05/01/12    22:54:54     5004
      SUBROUTINE DPSAT1(MPOVA1,MPOVA3)
C--------------------------------------------------------------------
C Derivee de la pression partielle de vapeur à saturation
C--------------------------------------------------------------------
C
C  E/   MPOVA1 : Température de saturation (en K)
C   /S  MPOVA3 : Derivee de la pression partielle de vapeur
C                à saturation (en Pa/K)
C
C--------------------------------------------------------------------
C
C Langage : ESOPE + FORTRAN77
C 12/2002
C
C-----------------------------------------------------------------------
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
C
-INC SMCHPOI
C
      N1 = MPOVA1.VPOCHA(/1)
      DO 10 I=1,N1
         X1 = MPOVA1.VPOCHA(I,1)
         X3 = DPSAT0(X1)
         MPOVA3.VPOCHA(I,1) = X3
 10   CONTINUE
C
      RETURN
      END



