C AVENDO    SOURCE    CHAT      05/01/12    21:30:23     5004
      SUBROUTINE AVENDO(EP,END,DEQ)
C=======================================================================
C     CALCUL DE DEQ
C=======================================================================
C
C     CREATION : F.CORMERY
C                E.N.S.M.A - LMPM
C                DEC 1992
C=======================================================================
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8(A-H,O-Z)
      REAL*8 LAMB,MUT
C-----------------------------------------------------------------------
C                                                   COMMONS SPECIFIQUES
C-----------------------------------------------------------------------
      COMMON/APENTE/ LAMB,MUT,G,ALP1,BETA
      COMMON/MAED/ B,CO,CPRIME
C-----------------------------------------------------------------------
C-------                                              DIMENSION ET DATA
C-----------------------------------------------------------------------
      DIMENSION EP(*),END(*)
      DATA DEUX/2.D0/
C-----------------------------------------------------------------------
C------                                            ---------------------
C------                                              TRACE DES PRODUITS
C------                                            ---------------------
      TRAC1=EP(1)**2+EP(2)**2+EP(3)**2
     *             +(EP(4)**2+EP(5)**2+EP(6)**2)/DEUX

      TRAC2=EP(1)*END(1)+EP(2)*END(2)+EP(3)*END(3)
     *  +EP(4)*END(4)+EP(5)*END(5)+EP(6)*END(6)

C------                                            --------------------
C------                                             DOMMAGE EQUIVALENT
C------                                            --------------------
      DEQ=SQRT((G**2/DEUX)*TRAC1)-B*G*TRAC2

C-----------------------------------------------------------------------
      RETURN
      END



