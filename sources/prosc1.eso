C PROSC1    SOURCE    CHAT      05/01/13    02:34:53     5004
      SUBROUTINE PROSC1(VECTB,VECTC,X,LONVEC)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8(A-H,O-Z)
C SOUS-PROGRAMME APPELE PAR PROJ3
      DIMENSION VECTB(1),VECTC(1)
      X=0.D0
      DO 1 I=1,LONVEC
      X=X+VECTB(I)*VECTC(I)
    1 CONTINUE
      RETURN
      END

