C TRJCN4    SOURCE    CHAT      05/01/13    03:49:11     5004
      SUBROUTINE TRJCN4(TMIN,TMAX,DELTAT)
CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C
C        CE SOUS PROGRAMME CONTROLE LA COHERENCE ENTRE L'INTERVALLE
C        DE TEMPS ET LE PAS
C        TMIN  TEMPS INITIAL
C        TMAX  TEMPS MAXIMUM POSSIBLE
C        DELTAT PAS DE TEMPS
C
CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
      IMPLICIT INTEGER(I-N)
       IMPLICIT REAL*8 (A-H,O-Z)
      VINT=TMAX-TMIN
      S1=SIGN(1.D0 ,VINT)
      S2=SIGN(1.D0 ,DELTAT)
C     write(6,*)' s1 s2 ' ,s1,s2
      IF(S1.NE.S2)THEN
           CALL ERREUR(481)
           RETURN
      ENDIF
      RETURN
      END


