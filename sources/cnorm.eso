C CNORM     SOURCE    PV        07/11/23    21:15:59     5978
C   CALCUL LA NORME D'UN VECTEUR
C
      SUBROUTINE CNORM
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (a-h,o-z)
-INC CCOPTIO
-INC SMCOORD
      REAL*8 XNA
      CALL LIROBJ('POINT   ',IP,1,IRETOU)
      IF (IERR.NE.0) RETURN
      SEGACT MCOORD
      IREF=(IP-1)*(IDIM+1)
      XNO=0.D0
      DO 1 I=1,IDIM
      XNO=XNO+XCOOR(IREF+I)**2
   1  CONTINUE
      XNA=SQRT(XNO)
      CALL ECRREE(XNA)
      RETURN
      END


