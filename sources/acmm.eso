C ACMM      SOURCE    CHAT      05/01/12    21:15:53     5004
      SUBROUTINE ACMM(ITABLE,MOTI,MOT1)
C-----------------------------------------------------------------------
C Récupération d'un MOT stocké à un indice de type MOT d'une TABLE.
C-----------------------------------------------------------------------
C
C------------------------
C Opérandes et résultat :
C------------------------
C
C E/  ITABLE : Table
C E/  MOTI   : Indice de type MOT indexant l'objet cherché
C  /S MOT1   : MOT contenu dans la table ITABLE à l'indice MOT MOTI
C
C-----------------------------------------------------------------------
C ATTENTION : Le résultat MOT1 et l'indice MOTI sont des mots d'au plus
C             8 caractères (sinon voir ACCTAB). Les blancs à la fin de
C             MOTI sont ignorés ('mot sb  ' est transformé en 'mot sb')
C-----------------------------------------------------------------------
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8(A-H,O-Z)
      LOGICAL XLOGI
      CHARACTER*(*) MOTI,MOT1
      CHARACTER*8 TYPE
C
C      MOT  = MOTI
      TYPE = 'MOT     '
      CALL LENCHA(MOTI,LC)
      CALL ACCTAB(ITABLE,'MOT',0,0.D0,MOTI(1:LC), .TRUE.,0,
     1                    TYPE,IENT,XVAL,MOT1,XLOGI,IRET)
      RETURN
      END



