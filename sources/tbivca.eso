        SUBROUTINE TBIVCA ( IXA,IYA,IXB,IYB, TABTR )

*
*  DEFINITION DES VARIABLES
*
      IMPLICIT INTEGER(I-N)
-INC TMNTAB
        INTEGER         IXA,IXB,IYA,IYB
        CHARACTER*30    TMPCAR
*
          TMPCAR = TABTR.ELEM(IXA,IYA)
          TABTR.ELEM(IXA,IYA) = TABTR.ELEM(IXB,IYB)
          TABTR.ELEM(IXB,IYB) = TMPCAR
*
        END


