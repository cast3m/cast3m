C ZVAP3     SOURCE    CHAT      05/01/13    04:25:22     5004
      SUBROUTINE ZVAP3(MLREE1,MLREE2,MLREE3)
C------------------------------------------------------------------------
C Correction gaz reel pour la vapeur (datas LISTREEL)
C------------------------------------------------------------------------
C
C  E/  MLREE1   : Densité de vapeur (en kg/m3)
C  E/  MLREE2   : Température (en K)
C   /S MLREE3   : Correction gaz reel pour la vapeur
C
C-----------------------------------------------------------------------
C
C Langage : ESOPE + FORTRAN77
C Auteurs : F.DABBENE 01/00
C
C-----------------------------------------------------------------------
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
C
-INC SMLREEL
C
      JG1 = MLREE1.PROG(/1)
      DO 10 I=1,JG1
         X1 = MLREE1.PROG(I)
         X2 = MLREE2.PROG(I)
         X3 = ZVAP0(X1,X2)
         MLREE3.PROG(I) = X3
 10   CONTINUE
C
      RETURN
      END


