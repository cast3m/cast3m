C TRJVLO    SOURCE    CHAT      05/01/13    03:51:59     5004
      SUBROUTINE TRJVLO(IZNOEU,IZUN,IZTRAV,ITYP,UELEM,NUPOIN,IEL,IZSH)
C
CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C
C     CALCUL DE LA VITESSE (REELLE) UELEM A L'INTERIEUR D'UN ELEMENT
C     EN FONCTION DES VITESSES NODALES
C
C     IZNOEU COORDONNEES ET NUMEROS DES NOEUDS DE L'ELEMENT
C     IZUN VITESSES AUX NOEUDS DE L'ELEMENT
C     IZTRAV COORDONNEES DES NOEUDS (ON S'INTERESSE AU NUPOIN-IEME )
C     ITYP TYPE DE L'ELEMENT
C     NUPOIN NUMERO DE LA PARTICULE A LAQUELLE ON S'INTERESSE
C     IEL NUMERO DE L ELEMENT
C     ITYP TYPE DE LA VITESSE
C     IZSH SEGMENT DE TRAVAIL POUR LES FONCTIONS DE FORME
C
C
CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C
C
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
C

      SEGMENT  IZNOEU
            REAL*8 XELE(IDIM,NOEL)
            INTEGER NOEGLO(NOEL)
      ENDSEGMENT
      SEGMENT IZTRAV
           REAL*8  COOR(NDIM,NPART)
      ENDSEGMENT
C
      SEGMENT IZSH
            REAL*8 SHP(6,MNO9),SHY(12,MNO9),XYZL(3,MNO9)
      ENDSEGMENT
C
      SEGMENT IZUN
           REAL*8 UN(I1,I2,I3)
      ENDSEGMENT
C
      DIMENSION UELEM(3),XYREF(3),XYZP(3)
C
      IDIM=XELE(/1)
      NOEL=XELE(/2)
C
      DO 1 ID=1,IDIM
        XYZP(ID)=COOR(ID,NUPOIN)
   1  CONTINUE
C
C
       IER=0
C                     TRI3
        IF(ITYP.EQ.4)CALL TRJ302(XELE,XYZP,XYREF)
C                     QUA4
        IF(ITYP.EQ.8)CALL TRJ402(XELE,XYZP,XYREF,IER)
C                     TRI6
        IF(ITYP.EQ.6)CALL TRJ602(XELE,XYZP,XYREF,IER)
C                     TRI7
        IF(ITYP.EQ.7)CALL TRJ702(XELE,XYZP,XYREF,IER)
C                     QUA9
        IF(ITYP.EQ.11)CALL TRJ902(XELE,XYZP,XYREF,IER)
C                     PRI6
        IF(ITYP.EQ.16)CALL TRJ603(XELE,XYZP,XYREF,IER)
C                     CUB8
        IF(ITYP.EQ.14)CALL TRJ803(XELE,XYZP,XYREF,IER)
C                     TET4
        IF(ITYP.EQ.23)CALL TRJ403(XELE,XYZP,XYREF)
      IF(IER.NE.0)THEN
                   CALL ERREUR(IER)
      ENDIF
C
C
        CALL TRJVPO(UN(1,1,IEL),UELEM,XYREF,ITYP,IDIM,NOEL,SHP)
      RETURN
      END



