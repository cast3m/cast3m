C LEMAIT    SOURCE    CHAT      05/01/13    01:15:12     5004
      SUBROUTINE LEMAIT(SIG,X,DX,DT,XMAT,DEPCT)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL *8 (A-H,O-Z)
      DIMENSION XMAT(*)
      DX= (SIG/XMAT(9 ))**(XMAT(8)/XMAT(7))*XMAT(11)**(1.0D0/XMAT(7))
      DY= (SIG/XMAT(10))**XMAT(8)*XMAT(11)
      XX=X
      T1=0.5D0*DX*DT
      IF(XX.LT.T1) XX=T1
      DEPCT=XMAT(6)*(XMAT(7)*XX**(XMAT(7)-1.0D0)*DX+DY)
      RETURN
      END

