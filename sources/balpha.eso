C BALPHA    SOURCE    CHAT      05/01/12    21:31:41     5004
      FUNCTION BALPHA(X,ROL,ROV)
C
C     operateur FUITE
C>>>  calcule le taux de vide
C     X    : titre massique
C     ROL  : masse volumique de l'eau liquide
C     ROV  : masse volumique de la vapeur
C
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
      BALPHA=X*ROL/(X*ROL+(1.-X)*ROV)
      RETURN
      END


