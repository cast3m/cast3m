      SUBROUTINE KNBEL(MELEME,NEL)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
C************************************************************************
C  Ce sous-programme renvoie le nombre d'elements d'un objet maillage
C
C************************************************************************
      CALL ECROBJ('MAILLAGE',MELEME)
      CALL NBEL
      CALL LIRENT(NEL,1,IRET)
      RETURN
      END

