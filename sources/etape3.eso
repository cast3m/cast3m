C ETAPE3    SOURCE    CHAT      05/01/12    23:46:14     5004
*
* CALCUL DE LA NORME D UN VECTEUR
*
      SUBROUTINE ETAPE3(V,N,XNORZ)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8(A-H,O-Z)
      DIMENSION V(*)
      XX = 0.D0
      DO 1 I= 1,N
        XX = V(I)*V(I) + XX
  1   CONTINUE
      XNORZ=SQRT(XX)
      RETURN
      END

