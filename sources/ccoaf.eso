      SUBROUTINE CCOAF(SI,XLAM,VCRI)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8(A-H,O-Z)
C
C----------------------------------------------
C     CETTE ROUTINE PERMET DE CALCULER LA     |
C     VALEUR DU CRITERE DE COMPRESSION        |
C----------------------------------------------
C
      VCRI=-SI
      IF(XLAM.LE.1.E-8) VCRI=-1.D0
      RETURN
      END

