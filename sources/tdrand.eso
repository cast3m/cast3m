      SUBROUTINE TDRAND(XRAN)
C-----------------------------------------------------------------------
C Appel du générateur de nombres aléatoires uniformément répartis entre
C 0. et 1., bornes exclues, en conservant les valeurs par défaut.
C-----------------------------------------------------------------------
C
C---------------------------
C Parametres Entree/Sortie :
C---------------------------
C
C  /S XRAN : Valeur comprise entre 0. et 1.
C
C-----------------------------------------------------------------------
C Appelé par : aleat1.eso (opérateur ALEA)
C              graco6.eso (opérateur RESO option GRAD)
C              brui.eso   (opérateur BRUI)
C-----------------------------------------------------------------------
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
C
      IDUM = 0
      XRAN = TDRAN1(IDUM)
C
      RETURN
      END


