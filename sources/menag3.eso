C MENAG3    SOURCE    PV        11/04/15    21:15:12     6945
C  SUPPRIMER LES SEGMENTS INDESIRABLES
C
      SUBROUTINE MENAG3(ISLIS,ISLI2)
      IMPLICIT INTEGER(I-N)
      SEGMENT ISLIS(NP)
      SEGMENT ISLI2(0)
*  LISTE DES SEGMENTS NOUVEAUX
      IRANG=0
      IRAN2=0
      IRAN3=0
  10  CONTINUE
      IRANG=IRANG+1
      IF (IRANG.GT.ISLIS(/1)) GOTO 15
  11  CONTINUE
      IRAN2=IRAN2+1
      IF (IRAN2.GT.ISLI2(/1)) GOTO 13
      IP1=ISLIS(IRANG)
      IP2=ISLI2(IRAN2)
      if (ip2.eq.0) goto 13
      IF (IP2.LT.IP1) GOTO 11
      IF (IP2.EQ.IP1) GOTO 10
  13  CONTINUE
      IRAN3=IRAN3+1
      ISLIS(IRAN3)=ISLIS(IRANG)
      IRAN2=IRAN2-1
      GOTO 10
  15  CONTINUE
      SEGDES ISLI2
*     WRITE(6,*) ' SEGMENT DE LA LISTE         ',ISLIS
*     WRITE(6,*) ' NOMBRE DE SEGMENTS INITIAUX ',ISLI2(/1)
*     WRITE(6,*) ' NOMBRE DE SEGMENTS ACTUELS  ',ISLIS(/1)
*     WRITE(6,*) ' SEGMENTS A SUPPRIMER        ',IRAN3
*     DO 20 I=1,IRAN3
*      WRITE (6,*) ' SEGMENT SUPPLEMENTAIRE ',ISLIS(I)
* 20  CONTINUE
      NP=IRAN3
      SEGADJ ISLIS
      END


