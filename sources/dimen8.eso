      SUBROUTINE DIMEN8 (IPOINT,LDIM)
C
C     DIMENSION D UN OBJET EVOLUTIO
C     C EST LE NOMBRE TOTAL DE  COURBES DANS L'OBJET EVOLUTIO
C
C     PAUROBALLY    14/12/88
C
      IMPLICIT INTEGER(I-N)
-INC SMEVOLL
C
      MEVOLL=IPOINT
      SEGACT MEVOLL
      LDIM=IEVOLL(/1)
      SEGDES MEVOLL
C
      RETURN
      END

