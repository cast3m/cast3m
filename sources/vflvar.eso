      SUBROUTINE VFLVAR (SIG,NDIMS,VAR,NDIMV,VALOR,NMODEL)
      IMPLICIT INTEGER(I-N)
      integer ndims,ndimv,nmodel
      real*8 sig(ndims),var(ndimv),valor
      if (nmodel.eq.8) then
           call vflmiehevar(var(1),valor)
      else
           valor = 0.D0
      endif
      return
      end


