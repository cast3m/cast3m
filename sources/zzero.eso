C Inicializa a 0 un vector
CCC
      SUBROUTINE ZZERO (X,N)
      IMPLICIT INTEGER(I-N)
      integer i,n
      real*8 x(n)
      do i=1,n
        x(i)=0.D0
      enddo
      return
      end


