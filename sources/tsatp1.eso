C TSATP1    SOURCE    CHAT      05/01/13    03:53:12     5004
      SUBROUTINE TSATP1(MPOVA1,MPOVA3)
C--------------------------------------------------------------------
C Température de saturation de la vapeur pour une pression partielle
C de vapeur donnée.
C--------------------------------------------------------------------
C
C  E/   MPOVA1 : Pression partielle de vapeur (en Pa)
C   /S  MPOVA3 : Température de saturation (en K)
C
C--------------------------------------------------------------------
C
C Langage : ESOPE + FORTRAN77
C Auteurs : F.DABBENE 01/00
C
C-----------------------------------------------------------------------
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
C
-INC SMCHPOI
C
      N1 = MPOVA1.VPOCHA(/1)
      DO 10 I=1,N1
         X1 = MPOVA1.VPOCHA(I,1)
         X3 = TSATP0(X1)
         MPOVA3.VPOCHA(I,1) = X3
 10   CONTINUE
C
      RETURN
      END


