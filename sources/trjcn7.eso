C TRJCN7    SOURCE    CHAT      05/01/13    03:49:20     5004
      SUBROUTINE TRJCN7(TTC,IZVIT)
CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C
C        CE SOUS PROGRAMME CONTROLE LA COHERENCE ENTRE LE TEMPS
C        ETUDIE ET LES TEMPS AUQUELS LES VITESSES SONT DONNEES
C        TTC   TEMPS ETUDIE
C        IZVIT POINTEUR DU SEGMENT  DES VITESSES OU FLUX
C
CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
      IMPLICIT INTEGER(I-N)
       IMPLICIT REAL*8 (A-H,O-Z)
      SEGMENT IZVIT
           REAL*8 TEMTRA(NVIPT)
           INTEGER IPUN(NBS),IDUN(NBS),IPVPT(NVIPT),IFORML
      ENDSEGMENT
C             IDUN(I) nombre d elements avant le sous maillage I
C             IPVPT  pointeurs de izvpt pour chaque pas de temps
      SEGMENT IZVPT
           INTEGER IPUN1(NBS),IPUMAX
      ENDSEGMENT
C
      SEGACT IZVIT
      NVIPT= TEMTRA(/1)
      IF(NVIPT.NE.1)THEN
C                CAS  TRANSITOIRE
            IF(TTC.LT.TEMTRA(1))THEN
                 CALL ERREUR(-270)
                 CALL ERREUR(21)
                 RETURN
            ENDIF
            IF(TTC.GT.TEMTRA(NVIPT))THEN
                 CALL ERREUR(-270)
                 CALL ERREUR(21)
                 RETURN
            ENDIF
      ENDIF
      SEGDES IZVIT
      RETURN
      END


