C ELLA08    SOURCE    CHAT      05/01/12    23:33:51     5004
      SUBROUTINE ELLA08(COMP,ICOMP,IERROR)
C
C CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C
C   OPERATEUR ELFE LAPLACE ACOU
C
C   CONVERTION TYPE DE DONNEES DE CARACTERISTIQUE EN RANG DANS CARACT
C
C     YOUN --> 1
C     NU   --> 2
C     RHO  --> 3
C     RINT --> 4
C     REXT --> 5
C     KCYZ --> 6
C     CAM  --> 7
C     ETA  --> 8
C     RHOF --> 9
C     CSON --> 10
C     VECT --> 11
C
C   PARAMETRES :
C   COMP    : TYPE DE CARACTERISTIQUE (CHARACTER*8)
C
C   SORTIES :
C
C   ICOMP   : ENTIER CALCULE SUIVANT LE CODE PRECEDENT
C
C   IERROR  : ENTIER VALANT 0 OU 1 SI LA CONVERSION S'EST FAITE SANS
C             OU AVEC UNE ERREUR
C
C
C   AUTEURS : SAINT-DIZIER ET GORCY
C   DATE    : 24 JANVIER 1991
C
C CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C
C
      IMPLICIT INTEGER(I-N)
      INTEGER ICOMP,IERROR
      CHARACTER*8 COMP
C
      ICOMP = 0
      IERROR = 0
C
      IF      (COMP.EQ.'YOUN')  THEN
          ICOMP = 1
      ELSE IF (COMP.EQ.'NU')  THEN
          ICOMP = 2
      ELSE IF (COMP.EQ.'RHO')  THEN
          ICOMP = 3
      ELSE IF (COMP.EQ.'RINT')  THEN
          ICOMP = 4
      ELSE IF (COMP.EQ.'REXT')  THEN
          ICOMP = 5
      ELSE IF (COMP.EQ.'KCYZ')  THEN
          ICOMP = 6
      ELSE IF (COMP.EQ.'CAM')  THEN
          ICOMP = 7
      ELSE IF (COMP.EQ.'ETA')  THEN
          ICOMP = 8
      ELSE IF (COMP.EQ.'RHOF')  THEN
          ICOMP = 9
      ELSE IF (COMP.EQ.'CSON')  THEN
          ICOMP = 10
      ELSE IF (COMP.EQ.'VECT')  THEN
          ICOMP = 11
      ELSE
          IERROR = 1
C
      END IF
C
      END

