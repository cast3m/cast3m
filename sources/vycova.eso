      SUBROUTINE VYICOMACVAR (SIG,NDIMS,VARINT,VALOR)
      IMPLICIT INTEGER(I-N)
      integer ndims
      real*8    sig(ndims),varint,valor,etaconfuncder,p
      real*8    exm,qaa,pcc,eww,xk1,xk2,etb,exv,xep,paa,cco,
     .          exl,xnn,aaa,bbb
      common /MRScone/ exm,qaa,pcc,eww,xk1,xk2,etb,exv,xep,paa,cco,
     .                 exl,xnn,aaa,bbb
      p=-(sig(1)+sig(2)+sig(3))/3.D0
      valor=-(p-pcc)*etaconfuncder(varint)
      return
      end



