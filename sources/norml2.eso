CCC
C Norma: (suma cuadrados)/2
CCC
             FUNCTION NORM_L2_2(R,N)
      IMPLICIT INTEGER(I-N)
      integer i,n
      real*8 r(n),aux,norm_l2_2
      aux=0.D0
      do i=1,n
       aux=aux+r(i)*r(i)
      enddo
      norm_l2_2=aux/2.D0
      return
      end



