      SUBROUTINE UTPRIN(B,N1,N2)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
      DIMENSION B(N1,N2)
C
C   IMPRESSION D UNE MATRICE
C
      WRITE(6,*)  ' '
      DO 1 I = 1,N1
C     WRITE(6,*)  ' I ',I
      WRITE(6,100) I,(B(I,J),J=1,N2)
  1   CONTINUE
      WRITE(6,*) ' '
 100  FORMAT(2X,I3,10(1X,1PE11.4))
      RETURN
      END

