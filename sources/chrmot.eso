C CHRMOT    SOURCE    CHAT      05/01/12    22:02:05     5004
      SUBROUTINE CHRMOT(IMOT,LMOT,MOT,IRET)
C
C    CHERCHE LE MOT 'MOT', DANS LE TABLEAU IMOT DE LONGUEUR LMOT
C    ECRIT PAR FARVACQUE EN FORTRAN
C
      IMPLICIT INTEGER(I-N)
      CHARACTER*4    IMOT(*)
      CHARACTER*4     MOT
      IRET=0
      DO 1 I=1,LMOT
      IF(IMOT(I).NE.MOT) GOTO 1
      IRET=I
      GOTO 2
   1  CONTINUE
   2  CONTINUE
      RETURN
      END

