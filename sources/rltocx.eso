C RLTOCX    SOURCE    CHAT      05/01/13    03:03:33     5004
      FUNCTION RLTOCX(XRE,XIM)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
*
*   VERSION SUPPOSEE PORTABLE DE LA CONVERSION DE 2
*   REELS 8 EN UN COMPLEXE 16
*   M. PETIT LE 13/2/91
*
      REAL*8 XRE,XIM
      COMPLEX*16 RLTOCX,ZCMP
      DIMENSION XX(2)
      EQUIVALENCE (ZCMP,XX(1))
      XX(1)=XRE
      XX(2)=XIM
      RLTOCX=ZCMP
      RETURN
      END

