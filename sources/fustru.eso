      SUBROUTINE FUSTRU(MSTRU1,MSTRU2,MSTRUC)
C
C     CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C     C CE SUBROUTINE REALISE L OPERATION "ET" SUR LES 2 OBJETS        C
C     C STRUCTURES MSTRU1 ET MSTRU2. LE RESULTAT EST RANGE DANS MSTRUC C
C     CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C
      IMPLICIT INTEGER(I-N)
-INC SMSTRUC
C
      SEGACT MSTRU1,MSTRU2
      NSTRU1=MSTRU1.LISTRU(/1)
      NSTRU2=MSTRU2.LISTRU(/1)
      N=NSTRU1+NSTRU2
      SEGINI MSTRUC
      DO 3 I=1,NSTRU1
      LISTRU(I)=MSTRU1.LISTRU(I)
 3    CONTINUE
      DO 4 J=1,NSTRU2
      I=NSTRU1+J
      LISTRU(I)=MSTRU2.LISTRU(J)
 4    CONTINUE
      SEGDES MSTRUC,MSTRU1,MSTRU2
      RETURN
      END

