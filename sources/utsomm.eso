C UTSOMM    SOURCE    CHAT      05/01/13    04:00:22     5004
      SUBROUTINE UTSOMM(A,N,AT)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
      DIMENSION A(N)
C
C   SOMME DES TERMES D' VECTEUR
C
C     WRITE(6,*)  ' '
      AT = 0.
      DO 1 I = 1,N
      AT = AT + A(I)
  1   CONTINUE
      RETURN
      END

