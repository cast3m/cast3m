C SCAL2     SOURCE    CHAT      05/01/13    03:12:22     5004
      SUBROUTINE SCAL2(ENT,SOR,VAL)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8(A-H,O-Z)
      DIMENSION ENT(3),SOR(3)
C
C-----------------------------------------------------
C     CALCUL DE LE PRODUIT SCALAIRE  SIG * EPS
C               POUR LE MODELE BETON
C-----------------------------------------------------
C
      VAL=SOR(1)*ENT(1)+SOR(2)*ENT(2)+2.D0*SOR(3)*ENT(3)
      RETURN
      END

