C TRJFAC    SOURCE    CHAT      05/01/13    03:49:39     5004
      SUBROUTINE TRJFAC(IEL,NF,JFA,IELTFA)
CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C
C      RECHERCHE JFA NUMERO DE LA FACE NF DANS L ELEMENT IEL
C       A PARTIR  DE LA TABLE GENEREE PAR KDOM
C
C
C
CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
-INC SMELEME
      POINTEUR IELTFA.MELEME
C
C
      CALL MELNEL(IEL,IELTFA,IPT2,NEL2,1)
      NP2=IPT2.NUM(/1)
      DO 20 LL=1,NP2
        IF(IPT2.NUM(LL,IEL-NEL2).EQ.NF)THEN
             JFA=LL
             GO TO 55
        ENDIF
   20 CONTINUE
   55 CONTINUE
      RETURN
      END

