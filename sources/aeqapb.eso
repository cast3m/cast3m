C AEQAPB    SOURCE    CHAT      05/01/12    21:19:04     5004
      SUBROUTINE AEQAPB(A,B,LA)
C
C         CALCUL DE A=A+B
C         A ET B TABLEAUX DIMENSIONNES A LA
C
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8(A-H,O-Z)
      DIMENSION A(*),B(*)
      DO 1 IA=1,LA
    1 A(IA)=A(IA)+B(IA)
      RETURN
      END

