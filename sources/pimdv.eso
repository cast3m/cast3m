C PIMDV     SOURCE    CHAT      05/01/13    02:13:55     5004
      SUBROUTINE PIMDV (A,B,C,NL)
C
C----------------------------------------------------------
C PRODUIT DE LA MATRICE DIAGONALE A ET DU VECTEUR B ---> C
C----------------------------------------------------------
C
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
C
      DIMENSION A(1),B(1),C(1)
C
      DO 10 I=1,NL
      C(I)=A(I)*B(I)
  10  CONTINUE
C
      RETURN
      END

