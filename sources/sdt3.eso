C SDT3      SOURCE    CHAT      05/01/13    03:13:10     5004
      FUNCTION SDT3(N,A,B,C)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
      REAL*8 SDT3,AUX,A(1),B(1),C(1)
C
      AUX=0.D0
      DO 1 I=1,N
      AUX=AUX+A(I)*B(I)*C(I)
 1    CONTINUE
      SDT3=AUX
      RETURN
      END


