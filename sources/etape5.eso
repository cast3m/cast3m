C ETAPE5    SOURCE    CHAT      05/01/12    23:46:21     5004
      SUBROUTINE ETAPE5 (MLREEL,MLREE1,MXMAT,M)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8(A-H,O-Z)
*
*  CONJUGUAISON DES DIRECTIONS DE RECHERCHES
*
-INC SMLREEL
-INC TMXMAT
      CALL TXAY(MLREEL,MXMAT,MLREE1,M,M,AA)
      CALL TXAY(MLREE1,MXMAT,MLREE1,M,M,BB)
      BETA = - AA / BB
      DO 1 I=1,M
      PROG(I)=MLREE1.PROG(I) * BETA + PROG(I)
    1 CONTINUE
      RETURN
      END

