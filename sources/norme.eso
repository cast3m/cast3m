C NORME     SOURCE    CHAT      05/01/13    02:00:25     5004
      SUBROUTINE NORME(ENT,VAL)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8(A-H,O-Z)
      DIMENSION ENT(3)
C
C-----------------------------------------------------
C     CALCUL DE LA NORME D UN VECTEUR
C-----------------------------------------------------
C
      VAL=SQRT(ENT(1)*ENT(1)+ENT(2)*ENT(2)+ENT(3)*ENT(3))
      RETURN
      END

