C EPSPRE    SOURCE    CHAT      05/01/12    23:42:54     5004
      SUBROUTINE EPSPRE(X,Y,EPS,ICLE)
C
C         VERIFIE SI Y EST EGAL A X A PES PRES
C
C         SI OUI ICLE =1
C         SI NON ICLE =0
C
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8(A-H,O-Z)
C
      ICLE=1
      YM=Y-EPS
      YP=Y+EPS
      IF(X.GE.YM.AND.X.LE.YP) RETURN
      ICLE=0
      RETURN
      END

