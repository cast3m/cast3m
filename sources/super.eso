      SUBROUTINE SUPER
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8(A-H,O-Z)
      CHARACTER*4 MOLIS(4)
-INC CCOPTIO
      DATA MOLIS/'RIGI','CHAR','MASS','DEPL'/
      NMOT=4
      CALL LIRMOT(MOLIS,NMOT,IRET,1)
      IF(IERR.NE.0) RETURN
      GO TO (1,2,3,4),IRET
   1  CALL SUPRI
      RETURN
   2  CALL SUPCHA
      RETURN
   3  CALL SUPMAS
      RETURN
   4  CALL SUPDEP
      RETURN
      END





