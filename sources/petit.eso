C PETIT     SOURCE    CHAT      05/01/13    02:13:12     5004
      SUBROUTINE PETIT
      IMPLICIT INTEGER(I-N)
-INC CCOPTIO
      LOGICAL IR
      REAL*8 X1,X2
      ICOD=1
      CALL LIRREE(X1,ICOD,IRETOU)
      IF(IERR.NE.0) RETURN
      CALL LIRREE(X2,ICOD,IRETOU)
      IF(IERR.NE.0) RETURN
      IR=X1.LT.X2
      CALL ECRLOG (IR)
      RETURN
      END

