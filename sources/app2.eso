C APP2      SOURCE    CHAT      10/12/16    21:15:02     6819
C
C====&===1=========2=========3=========4=========5=========6=========7==
C Commentaires : Subroutine permettant de définir la relation entre
C                le taux de corrosion macroscopique TC et le taux d'éner-
C                gie additionel à prendre en compte pour modifier l'endom-
C                magement dans le cas du modèle RICJOI2D
C                avec interface imparfaite
C Auteurs      : B. Richard
C Date         : Février 2008
C====&===1=========2=========3=========4=========5=========6=========7==
        SUBROUTINE APP2(X,Y,NDIM)
C
C-----DECLARATION GENERALE
C
        IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8(A-H,O-Z)
C
C-----DECLARATION DES TYPE DE VARIABLES EN ARGUMENTS
C
        REAL*8 XTC(20),XYR(20)
C
C-----VARIABLES LOCALES
C
        INTEGER I
C
C-----DEFINITION DES TABLEAUX DE REFERENCE
C
CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
        XTC( 1) = 0.00D0
        XTC( 2) = 1.62D0
        XTC( 3) = 5.61D0
        XTC( 4) = 8.58D0
        XTC( 5) = 9.45D0
        XTC( 6) = 11.58D0
        XTC( 7) = 12.35D0
        XTC( 8) = 12.66D0
        XTC( 9) = 16.54D0
        XTC(10) = 17.09D0
        XTC(11) = 18.06D0
        XTC(12) = 20.45D0
        XTC(13) = 22.36D0
        XTC(14) = 22.99D0
        XTC(15) = 25.07D0

        XYR( 1) =  0.00D0
        XYR( 2) =  0.00D0
        XYR( 3) =  2.80D4
        XYR( 4) =  3.20D4
        XYR( 5) =  4.00D4
        XYR( 6) =  3.50D4
        XYR( 7) =  2.50D4
        XYR( 8) =  2.25D4
        XYR( 9) = -7.00D3
        XYR(10) = -2.60D3
        XYR(11) = -6.00D4
        XYR(12) = -1.50D5
        XYR(13) = -3.50D5
        XYR(14) = -1.50D6
        XYR(15) = -3.00D6
CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC

C
C-----TEST SI LE DEGRE DE CORROSION MAXI IDENTIFIE
C       N EST PAS DEPASSE
C
        IF (X.GT.XTC(NDIM)) THEN
                Y = XYR(NDIM)
                RETURN
        END IF
C
C-----INTERPOLATION LINEAIRE PAR MORCEAU
C
        DO I=1,(NDIM-1)
                IF ((XTC(I).LE.X).AND.(X.LE.XTC(I+1))) THEN
                        Y=(XYR(I+1)-XYR(I))/(XTC(I+1)-XTC(I))*X+
     &        (XTC(I+1)*XYR(I)-XTC(I)*XYR(I+1))/(XTC(I+1)-XTC(I))
                        RETURN
                END IF
        END DO

      RETURN
        END






