C RSETIV    SOURCE    CHAT      05/01/13    03:07:43     5004
      SUBROUTINE RSETIV(V,NC,KK,T,NT)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
      REAL*8 V(NC,*),T(*)

      IF(NT.GT.NC)THEN
      WRITE(6,*)' ERREUR SUR LES DIMENSIONS DANS RSETIV ! ! !'
      RETURN
      ENDIF

      DO 1 I=1,NT
      V(I,KK)=T(I)
 1    CONTINUE

      RETURN
      END

