C LFCDEN    SOURCE    CHAT      05/01/13    01:15:42     5004
      SUBROUTINE LFCDEN(NBAND,LMAX,ITAB,IRETOU,IFORM)
      IMPLICIT INTEGER(I-N)
-INC CCOPTIO
      CHARACTER*4 ITAB(*)
      IRETOU=0
      IF (LMAX.EQ.0) GO TO 15
      IF(IFORM.EQ.1)READ(NBAND,8001,END=10,ERR=10)(ITAB(I),I=1,LMAX)
      IF(IFORM.EQ.0)READ(NBAND,END=10,ERR=10)(ITAB(I),I=1,LMAX)
 8001 FORMAT(16(1X,A4))
  15  RETURN
 10   IRETOU=1
      RETURN
      END



