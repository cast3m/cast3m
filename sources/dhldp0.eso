C DHLDP0    SOURCE    CHAT      05/01/12    22:48:48     5004
      FUNCTION DHLDP0(P,T)
C--------------------------------------------------------------------
C derivee partielle de Enthalpie liquide spécifique par rapport a P
C Issue de la fonction Enthalpie liquide HLS0(P,T)
C--------------------------------------------------------------------
C
C  E/  P       : Pression (en Pa)
C  E/  T       : Température (en K)
C   /S DHLDP0  : D/DP(Hl) Derivee partielle enthalpie spécifique
C                liquide par rapport à la pression P (en J/kg/Pa)

C--------------------------------------------------------------------
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)

C     DERIVEE DES COEFFICIENTS
C     ****************************************************
C     d/dP(hl1(P))
      DHL1  =  0.18637D-2
C     ****************************************************
C     d/dP(hl2(P))
      DHL2  =  ((2*0.3654D-12)*P)-(0.30413D-5)
C     ****************************************************
C     d/dP(hl3(P))
      DHL3  =  ((-0.95261D-8)*2*P)-(0.25785D0)
C     ****************************************************
C     COEFFICIENT hl4
      HL4  =  399.98D0
C     ****************************************************
C     TEMPERATURE T-T0
C     ****************************************************
      TL1  =  (T - 273.15D0)
C     ****************************************************
C     FORMULE EXACTE DE LA DERIVEE
      DHLDP0 = DHL1 +(DHL2 * TL1)+(DHL3/(HL4 - TL1))
      RETURN
      END



