C AEQBPC    SOURCE    CHAT      05/01/12    21:19:08     5004
      SUBROUTINE AEQBPC (A,B,C,X,Y,LA)
C
C CALCUL DE A=B*X+C*Y
C A B C VECTEUR DE LONGUEUR LA
C X ET Y REELS
C
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
C
      DIMENSION A(*),B(*),C(*)
C
      DO 1 IA=1,LA
 1    A(IA)=X*B(IA)+Y*C(IA)
      RETURN
      END

