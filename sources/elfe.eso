C ELFE      SOURCE    CHAT      05/01/12    23:31:45     5004
      SUBROUTINE ELFE
C
C =====================================================================
C
C   L' OPERATEUR ELFE CALCULE LES VIBRATIONS
C   A MOYENNE FREQUENCE EN UTILISANT UNE METHODE INTEGRALE AUX ELEMENTS
C   FRONTIERE.
C
C   SYNTAXE :
C
C   RESU1  = ELFE  |   TEST  | TEMPS      )
C                  |         |            ) SUPPRIMES 05/03/90
C                  |         | LAPLACE  ..)
C                  |
C                  |   LAPLACE | POUTRE  .....
C                  |           | EXTERNE     SUPPRIMEE 05/03/90
C                  |           | PLAQUE  ....
C                  |
C                  |   STATIQUE | POUTRE  ....!
C                  |            | PLAQUE  ....!  supprimes 15/12/92
C                  |
C                  |   TEMPS     POUTRE STRUC1  ATTAC1  TEMPS
C                                       DT1  CHARG1  ( M1
C                                       GREEN    ( NFOIS NN ) ;
C
C     OPTION TEST TEMPS
C     -----------------
C
C
C     RESU1  : OBJET DE TYPE TABLE
C     LTF    : OBJET DE TYPE LISTREEL (TEMPS)
C     GREEN  : OBJET EVOLUTION CONTENANT LES FONCTIONS DE GREEN
C     E      : MODULE DE YOUNG
C     RO     : MASSE VOLUMIQUE
C     S      : SECTION
C     IZ     : MOMENT D INERTIE SUIVANT L AXE Z
C     COUPLAGE: MOT CLE PERMETTANT UNE RESOLUTION AVEC COUPLAGE
C     DECOUPLAGE: MOT CLE PERMETTANT UNE RESOLUTION AVEC DECOUPLAGE
C
C     OPTION TEMPS
C     ------------
C     SOLUT1 : OBJET DE TYPE SOLUTION DYNAMIQUE
C     STRUC1 : OBJET DE TYPE STRUCTURE, ENSEMBLE DES ELEMENTS DE POUTRE
C     ATTAC1 : OBJET DE TYPE ATTACHE, LIAISONS ENTRE LES ELEMENTS
C     TEMPS  : FLOTTANT, VALEUR DU TEMPS DE CALCUL
C     DT1    : FLOTTANT, VALEUR DU PAS DE TEMPS
C     CHARG1 : OBJET DE TYPE CHARGEMENT
C     GREEN  : OBJET EVOLUTION CONTENANT LES FONCTIONS DE GREEN
C   FACULTATIVEMENT
C     M1     : ENTIER, INDICE DE TRONCATURE , SI 0 PAS DE TRONCATURE
C     NFOI   : MOT CLE
C     NN     : CHPOINT RESULTAT DANS L'OBJET SOLUTION TOUT LES NN PAS
C
C    VERSION     : JUIN 86
C    PROGRAMMEUR : GUILBAUD
C    MODIFIE PAR P.MANIGOT EN MARS 1988
C    MODIFIE PAR X.VACELET EN JUIN 1989
C    MODIFIE PAR e de langre dec 1992
C
C =====================================================================
C
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8(A-H,O-Z)
-INC CCOPTIO
      CHARACTER*40 CHA1
      CHARACTER*40 CHA2
C
      CALL LIRCHA(CHA1,1,IRETOU)
      IF (IERR.NE.0) RETURN
C
C
C
      IF (CHA1(1:5).EQ.'TEMPS') THEN
        CALL LIRCHA(CHA2,1,IRETOU)
        IF (IERR.NE.0) RETURN
        IF     (CHA2(1:6).EQ.'POUTRE') THEN
           CALL ELFTEM
          RETURN
        ENDIF
C
      ELSEIF (CHA1(1:7).EQ.'LAPLACE') THEN
        CALL LIRCHA(CHA2,1,IRETOU)
        IF (IERR.NE.0) RETURN
        IF (CHA2(1:6).EQ.'POUTRE') THEN
           CALL ELLP00
           RETURN
        ELSEIF (CHA2(1:4).EQ.'ACOU') THEN
           CALL ELLA00
           RETURN
        ELSEIF (CHA2(1:6).EQ.'PLAQUE') THEN
           CALL ELPDY2
           RETURN
        ENDIF
C
      ENDIF
C
      RETURN
C
      END

