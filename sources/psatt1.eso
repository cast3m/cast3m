C PSATT1    SOURCE    CHAT      05/01/13    02:37:03     5004
      SUBROUTINE PSATT1(MPOVA1,MPOVA3)
C--------------------------------------------------------------------
C Pression partielle de vapeur à saturation pour une température donnée
C--------------------------------------------------------------------
C
C  E/   MPOVA1 : Température de saturation (en K)
C   /S  MPOVA3 : Pression partielle de vapeur à saturation (en Pa)
C
C--------------------------------------------------------------------
C
C Langage : ESOPE + FORTRAN77
C Auteurs : F.DABBENE 01/00
C
C-----------------------------------------------------------------------
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
C
-INC SMCHPOI
C
      N1 = MPOVA1.VPOCHA(/1)
      DO 10 I=1,N1
         X1 = MPOVA1.VPOCHA(I,1)
         X3 = PSATT0(X1)
         MPOVA3.VPOCHA(I,1) = X3
 10   CONTINUE
C
      RETURN
      END


