C ECML      SOURCE    CHAT      05/01/12    23:24:14     5004
      SUBROUTINE ECML(ITABLE,MOTI,LLOG)
C
C ECRIRE UN LOGIQUE DESIGNE PAR UN MOT
C
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8(A-H,O-Z)
      CHARACTER*(*) MOTI
      CHARACTER*8 MOT
      LOGICAL     LLOG
      MOT=MOTI
      CALL LENCHA(MOT,LC)
      CALL ECCTAB(ITABLE,'MOT',0,0.D0,MOT(1:LC),LLOG,0,
     1                   'LOGIQUE',0,0.D0,MOT(1:LC),LLOG,0)
      RETURN
      END


