C PLACE     SOURCE    CHAT      05/01/13    02:14:42     5004
      SUBROUTINE PLACE(MOT,LMOT,IMOT,MOH)
C
C         CHERCHE LA PLACE DE MOH DS MOT
C         IMOT= PLACE DU MOT
C         IMOT=0  MOH NON TROUVE DS MOT
C
      IMPLICIT INTEGER(I-N)
      CHARACTER*(*) MOT(*),MOH
C
      IMOT=0
      DO 1 IA=1,LMOT
      JA=IA
      IF(MOH.EQ.MOT(IA))       GO TO 2
    1 CONTINUE
      RETURN
    2 IMOT=JA
      RETURN
      END

