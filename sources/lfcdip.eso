C LFCDIP    SOURCE    PV        05/04/13    21:15:44     5073
      SUBROUTINE LFCDIP(NBAND,LMAX,ITAB,IRET,IFORM)
C
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8(A-H,O-Z)
      DIMENSION ITAB(1)

-INC CCOPTIO
-INC CCFXDR

CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C
C     BUT : RELIRE LES EN-TETES DE PILES (CF ECDIFP)
C
CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC

      IRET=0
      IF (LMAX.EQ.0) GO TO 2

      IF (IFORM.EQ.1) THEN
         IF(IONIVE.LE.7) THEN
            READ(NBAND,8004,END=1,ERR=1) (ITAB(I),I=1,LMAX)
         ELSE
            READ(NBAND,8008,END=1,ERR=1) (ITAB(I),I=1,LMAX)
         ENDIF
      ENDIF
      IF (IFORM.EQ.0) READ(NBAND,END=1,ERR=1) (ITAB(I),I=1,LMAX)
      if (iform.eq.2) ios= IXDRIMAT( ixdrr, lmax,itab)
      GO TO 2
    1 IRET=1
    2 RETURN

C8004 FORMAT(' PILE NUMERO',I4,' NBRE OBJETS NOMMES',I4,' NBRE OBJETS',
C    & I4)
 8004 FORMAT(12X,I4,19X,I4,11X,I5)
 8008 FORMAT(12X,I4,18X,I8,11X,I8)
      END



