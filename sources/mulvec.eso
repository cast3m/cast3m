C MULVEC    SOURCE    CHAT      05/01/13    01:55:41     5004
      SUBROUTINE MULVEC(Y,X,FT,L)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8(A-H,O-Z)
C
C  =====================================================================
C    SOUS-PROGRAMME FORTRAN APPELE PAR ELFRES
C    MULTIPLICATION DU VECTEUR X PAR LE COEF FT RESULTAT Y
C    CREATION    :  4/11/86
C    PROGRAMMEUR : GUILBAUD
C  =====================================================================
C
      DIMENSION Y(1),X(1)
      DO 10 I=1,L
      Y(I)=FT*X(I)
   10 CONTINUE
      RETURN
      END

