C HPRIME    SOURCE    CHAT      05/01/13    00:26:20     5004
      SUBROUTINE HPRIME(XE,NBNN,IDIM,SHPTOT,IGAU,SHPWRK,DJAC)
C=======================================================================
C
C   ENTREES
C     XE(3,NBNN)= COORDONNEES DES NOEUDS
C     NBNN      = NOMBRE DES NOEUDS
C     IDIM      = DIMENSION DU PROBLEME
C     SHPTOT(6,NBNO,IGAU)= FONCTIONS DE FORME REDUITES (VIENT DE MINTE
C     IGAU      =NUMERO DU POINT DE GAUUS
C  SORTIES
C     SHPWRK(6,NBNO)= H  H,X   H,Y   H,Z
C     DJAC      =JACOBIEN
C
C=======================================================================
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8(A-H,O-Z)
      DIMENSION XE(3,*),SHPTOT(6,NBNN,*),SHPWRK(6,*)
      IDIM1=IDIM+1
      DO 100 IA=1,NBNN
      DO 100 IB=1,IDIM1
      SHPWRK(IB,IA)=SHPTOT(IB,IA,IGAU)
  100 CONTINUE
      CALL JACOBI(XE,SHPWRK,IDIM,NBNN,DJAC)
      RETURN
      END

