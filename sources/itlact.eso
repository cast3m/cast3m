C ITLACT    SOURCE    PV        10/06/15    21:15:14     6692
      SUBROUTINE ITLACT(ICPR,ITLAC,IFAIT,ILISSE)
      IMPLICIT INTEGER(I-N)
      SEGMENT ICPR(N)
      SEGMENT ITLAC(M)
      MOLD = ITLAC(/1)
      M = IFAIT
      SEGADJ ITLAC
      DO 1 IA=1,ICPR(/1)
      IB=ICPR(IA)
      IF(IB.LE.MOLD) GO TO 1
       ITLAC(IB) = IA
   1  CONTINUE
      RETURN
      END





