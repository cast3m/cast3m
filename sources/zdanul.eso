C ZDANUL    SOURCE    CHAT      05/01/13    04:22:06     5004
      SUBROUTINE ZDANUL(A,LA)
C
C         ANNULE UN TABLEAU DE LONGUEUR LA        A EN DOUBLE PRECISION
C
      IMPLICIT INTEGER(I-N)
      REAL*8 A
      DIMENSION A(1)
      DO 1 IA=1,LA
      A(IA)=0.0D0
    1 CONTINUE
      RETURN
      END

