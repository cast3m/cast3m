      NOMTOT(1) = 'P1'
      NOMTOT(2) = 'P2'
      IF(IDIM .EQ. 3) NOMTOT(3) = 'P3  '
      CALL QUEPOI(IALUVC, ICEN, INDIC, NBCOMP, NOMTOT)
      IF(IERR .NE. 0)THEN
