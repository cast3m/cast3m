         NOMTOT(2) = 'P1DY'
         NOMTOT(3) = 'P2DX'
         NOMTOT(4) = 'P2DY'
      ELSE
         NBCOMP = 9
         NOMTOT(1) = 'P1DX'
         NOMTOT(2) = 'P1DY'
         NOMTOT(3) = 'P1DZ'
         NOMTOT(4) = 'P2DX'
         NOMTOT(5) = 'P2DY'
         NOMTOT(6) = 'P2DZ'
         NOMTOT(7) = 'P3DX'
         NOMTOT(8) = 'P3DY'
         NOMTOT(9) = 'P3DZ'
      ENDIF
      CALL QUEPOI(IGRULC, ICEN, INDIC, NBCOMP, NOMTOT)
      IF(IERR .NE. 0)THEN
C
C******* Message d'erreur standard
C        -301 0 %m1:40
C
         MOTERR(1:40)  = 'CHPO5 = ???                             '
         CALL ERREUR(-301)

         GOTO 9999
