C
C**** Control du CHPOINT: QUEPOI
C
      INDIC = 1
      IF(IDIM .EQ.2)THEN
         NBCOMP = 4
         NOMTOT(1) = 'P1DX'
         NOMTOT(2) = 'P1DY'
         NOMTOT(3) = 'P2DX'
         NOMTOT(4) = 'P2DY'
      ELSE
