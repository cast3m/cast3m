      SUBROUTINE LIRENT(IVAL , ICODE , IRETOU )
      IMPLICIT INTEGER(I-N)
-INC CCOPTIO
      CHARACTER*(8) IEN
      IEN='ENTIER  '
      CALL LIROBJ(IEN ,IRAT,ICODE,IRETOU)
      IF(IERR.NE.0) RETURN
      IF(IRETOU.EQ.0) RETURN
      IVAL=IRAT
      RETURN
      END



