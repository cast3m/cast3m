C GAMPO     SOURCE    CHAT      05/01/13    00:15:35     5004
      FUNCTION GAMPO(S,DS,R)
C
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
C
      DIMENSION S(*),DS(*)
C
      TRDSIG=DS(1)+DS(2)+DS(3)
      IF(TRDSIG.NE.0.D0) GO TO 1
      GAMPO=100.D0
      RETURN
    1 A=-0.333333333333333333333333D0
      TRSIG0=S(1)+S(2)+S(3)
      GAMPO=(R-A*TRSIG0)/(A*TRDSIG)
C
      RETURN
      END


