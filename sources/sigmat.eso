C SIGMAT    SOURCE    CB215821  16/04/21    21:18:27     8920
      FUNCTION SIGMAT(PRANDT,YPLUS,APLUS,BPLUS)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
      PR2=SQRT(PRANDT)
      DEN=(1.0-1.0/EXP(YPLUS*PR2/BPLUS+1.0E-5))
      RNUM=(1.0-1.0/EXP(YPLUS/APLUS+1.0E-5))
      SIGMAT=RNUM/DEN
      RETURN
      END



