C PLAMO8    SOURCE    CB215821  18/09/24    21:15:04     9931
      SUBROUTINE PLAMO8(MOT,LMOT,IMOT,MOH)
C
C         CHERCHE LA PLACE DE MOH DS MOT de taille 8
C         IMOT= PLACE DU MOT
C         IMOT=0  MOH NON TROUVE DS MOT
C
      IMPLICIT INTEGER(I-N)
      CHARACTER*(8) MOT(*),MOH

      IMOT=0
      DO IA=1,LMOT
        IF(MOH.EQ.MOT(IA)) THEN
          IMOT=IA
          RETURN
        ENDIF
      ENDDO
      END

