C LFCDIN    SOURCE    PV        05/04/13    21:15:42     5073
      SUBROUTINE LFCDIN(NBAND,LMAX,ITAB,IRETOU,IFORM)
      IMPLICIT INTEGER(I-N)
-INC CCOPTIO
-INC CCFXDR
      CHARACTER*8 ITAB(*)
      IRETOU=0
      IF (LMAX.EQ.0) GO TO 15
      IF(IONIVE.LE. 2) THEN
      IF(IFORM.EQ.1)READ(NBAND,801,END=10,ERR=10)(ITAB(I),I=1,LMAX)
      IF(IFORM.EQ.0)READ(NBAND,END=10,ERR=10)(ITAB(I),I=1,LMAX)
 801  FORMAT(16(1X,A4))
      ELSE
      IF(IFORM.EQ.1)READ(NBAND,8001,END=10,ERR=10)(ITAB(I),I=1,LMAX)
      IF(IFORM.EQ.0)READ(NBAND,END=10,ERR=10)(ITAB(I),I=1,LMAX)
 8001 FORMAT(8(1X,A8))
      ENDIF
      if (iform.eq.2) ios=IXDRSTRING( ixdrr, itab(1)(1:8*lmax))
  15  RETURN
 10   IRETOU=1
      RETURN
      END



