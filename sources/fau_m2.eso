C FAU_M2    SOURCE    BECC      10/05/05    21:15:05     6674
      function fau_m2(M,coef)
C
      real*8 fau_m2, M, coef
C
C     coef = 1.0D0 for m1_plus
C     coef = -1.0D0 for m1_minus
C
      fau_m2 = M + coef
      fau_m2 = fau_m2 * fau_m2
      fau_m2 = coef * 0.25D0 * fau_m2
C
      return
      end



