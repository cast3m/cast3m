C TRJCN2    SOURCE    CHAT      05/01/13    03:49:04     5004
      SUBROUTINE TRJCN2(MELEME)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C
C         CONTROLE QUE LE MAILLAGE MELEME CONVIENT A LA DESCRIPTION
C         DU LACHER POUR LE CALCUL DES TRAJECTOIRES
C     ON DOIT AVOIR UN MAILLAGE DE POI1
C
CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
-INC SMELEME
-INC CCOPTIO
C
      SEGACT MELEME
      NBSOUS=LISOUS(/1)
      IF(NBSOUS.NE.0)THEN
           INTERR(1)=MELEME
           CALL ERREUR(793)
           RETURN
      ENDIF
      IF(ITYPEL.NE.1)THEN
           INTERR(1)=MELEME
           CALL ERREUR(793)
           RETURN
      ENDIF
C
      SEGDES MELEME
      RETURN
      END


