      subroutine verifi
      implicit real*8 (a-h,o-z)
-INC CCOPTIO
*
      call lirree (xval,0,iretou)
       if (iretou.eq.1) then
          call verree(xval)
          return
       endif
      call lirobj ('CHPOINT  ',iret,0,iretou)
       if (iretou.eq.1) then
          call verchp(iret)
          return
       endif
      end









































































































