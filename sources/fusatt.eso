C FUSATT    SOURCE    CHAT      05/01/13    00:11:48     5004
      SUBROUTINE FUSATT(MATTA1,MATTA2,MATTAC)
C
C   CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C   C CE SUBROUTINE REALISE L OPERATION ''ET'' SUR LES DEUX OBJETS C
C   C ATTACHES MATTA1 ET MATTA2. LE RESULTAT EST RANGE DANS MATTAC C
C   CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C
      IMPLICIT INTEGER(I-N)
-INC SMATTAC
      SEGACT MATTA1,MATTA2
      N1=MATTA1.LISATT(/1)
      N2=MATTA2.LISATT(/1)
      N=N1+N2
      SEGINI MATTAC
      DO 3 I=1,N1
      LISATT(I)=MATTA1.LISATT(I)
 3    CONTINUE
      DO 4 J=1,N2
      I=N1+J
      LISATT(I)=MATTA2.LISATT(J)
 4    CONTINUE
      SEGDES MATTAC,MATTA1,MATTA2
      RETURN
      END

