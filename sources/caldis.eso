C CALDIS    SOURCE    CB215821  16/04/21    21:15:27     8920
      FUNCTION  CALDIS(A,B,NES)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
      DIMENSION A(NES),B(NES)
C
C     CALCUL DE LA DISTANCE ENTRE DEUX POINTS
C
      D= 0.
      DO 1 I= 1,NES
      D = D + ( B(I)-A(I))*(B(I)-A(I))
  1   CONTINUE
      CALDIS = SQRT(D)
      RETURN
      END



