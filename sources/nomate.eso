*
*              Et si le materiau n'est pas totalement decode
*
                        CALL MODPLA(MOMODL,NMOD)
                        INMAT= INMAT+1
                        CALL PLACE(MOMODL,NMOD,IMOD,MATMOD(INMAT))
                        IF (IMOD.EQ.0) THEN
                           CMATE=' '
                           IMATE=0
                           INATU=0
