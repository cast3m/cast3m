*     BETON_AXI
                           INATU=36
                        ELSE IF (IMOD.EQ.19) THEN
*     GURSON
                          INATU=38
                        ELSE IF (IMOD.EQ.20) THEN
*     BETON_UNI
                           INATU=39
                        ELSE IF (IMOD.EQ.21) THEN
*     ACIER_UNI
                           INATU=40
                        ELSE IF (IMOD.EQ.51) THEN
*     ACIER_ANCRAGE
                           INATU=93
                        ELSE IF (IMOD.EQ.23) THEN
*     OTTOSEN (BETON)
                           INATU=42
