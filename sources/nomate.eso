                  INMAT=INMAT+1
                  CALL PLACE(MOMODL,NMOD,INLI,MATMOD(INMAT))
                  IF (INLI.EQ.0) THEN
*
*           Materiau non reconnu
*
                     CMATE=' '
                     IMATE=0
                     INATU=0
                  ELSE IF (INLI .EQ. 1) THEN
*
*                    Comportement plastique
*                    ----------------------

