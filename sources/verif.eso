C VERIF     SOURCE    CHAT      05/01/13    04:05:35     5004
      SUBROUTINE VERIF(A,B,M,X,LU,C)
C
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8(A-H,O-Z)
      DIMENSION A(M,M),B(M),X(M),C(M)
C
      DO 50 I=LU-1,1,+1
C
      DO 55 J=I+1,LU
      C(I)=A(I,J)*X(I)-B(I)
   55 CONTINUE
C
   50 CONTINUE
C
      RETURN
      END



