C FTRC2     SOURCE    CHAT      05/01/13    00:11:08     5004
      FUNCTION FTRC2(Z,A,B,C)
      IMPLICIT INTEGER(I-N)
      IMPLICIT REAL*8 (A-H,O-Z)
      C12=SQRT(C)
      C32=C12**3
      Z2=Z**2
      TRINOM=A+B*Z+C*Z2
      TRINOM=SQRT(TRINOM)
      FTRC2=TRINOM/C-B/2.*LOG(TRINOM+Z*C12+B/(2.*C12))/C32
      RETURN
      END


