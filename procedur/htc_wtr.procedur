'SI'( 'EGA' M1 'SATU');
*  RESO1 = TA 'MASQUE' 'EGINFE' 374.15;
*  RESO2 = 1. - RESO1 ;
*
* Saturation pressure in Mpa
*
'SI'('NON'('EXIS' TAV1 'EVPS'));
TPR1= 'PROG' 0. 4. 5. 6. 8. 10. 'PAS' 1. 36.
            38. 40. 'PAS' 5. 100. 'PAS' 10. 300.
            320. 340. 360. 374.14 2000.;
PS21='PROG' .000611 .000813 .000872 .000935 .001072 .001228 .001312
    .001402 .001497 .001598 .001705 .001818 .001938 .002064 .002198
    .002339 .002487 .002645 .002810 .002985 .003169 .003363 .003567;
PS22='PROG' .003782 .004008 .004246 .004496 .004759 .005034 .005324
    .005628 .005947 .006632 .007384 .009593 .01235  .01576  .01994
    .02503  .03119  .03858  .04739  .05783  .07014  .08455  .10133
    .1433   .1985   .2701   .3613   .4758   .6178   .7917  1.002
   1.254   1.554   1.906   2.318   2.795   3.344   3.973   4.688
   5.499   6.412   7.436   8.581  11.27   14.59   18.65   22.09  22.09;
