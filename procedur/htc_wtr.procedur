'FINSI';
OUT= 'NOMC' 'SCAL' ('VARI' (TAV1.'EVPS') TA);
*
*  AAA=33661.805;
*  BBB=4744.058;
*  OUT1= RESO1 *(AAA*('EXP'(((TA + 273.15)**-1)*(-1.*BBB))));
*  OUT2= RESO2 *(AAA*('EXP'(((647.3)**-1)*(-1.*BBB)))) ;
*  OUT = OUT1 + OUT2;
*
'SINON';
 'SI'( 'EGA' M1 'RHOW');
* Density as fun. of T and P
   A0= 4.8863E-7 ;A1=-1.6528E-9 ;A2= 1.8621E-12;
   A3= 2.4266E-13;A4=-1.5996E-15;A5= 3.3703E-18;
   B0= 1.0213E3  ;B1=-7.7377E-1 ;B2= 8.7696E-3 ;
   B3=-9.2118E-5 ;B4= 3.3534E-7 ;B5=-4.4034E-10;
   PR= 2.E7;
   B0=B0 - (PR*A0);B1=B1 - (PR*A1);B2=B2 - (PR*A2);
   B3=B3 - (PR*A3);B4=B4 - (PR*A4);B5=B5 - (PR*A5);
   RO1=TA*(TA*(TA*(TA*(TA*B5+B4)+B3)+B2)+B1)+B0;
   OUT=PA*(TA*(TA*(TA*(TA*(TA*A5+A4)+A3)+A2)+A1)+A0)+RO1;
* mess 'density' (maxi out);
 'SINON';
  'SI'( 'EGA' M1 'HFG');
* Heat of vaporization J/Kg
*
    RESO1 = TA 'MASQUE' 'EGINFE' 374.15;
*
    OUT   = RESO1 * 350000.*(('ABS'(374.15 - TA))**(1./3.));
  'SINON';
   'SI'( 'EGA' M1 'CW');
* Specific heat J/Kg
     OUT   = 4100. ;
   'FINSI';
  'FINSI';
 'FINSI';
'FINSI';
