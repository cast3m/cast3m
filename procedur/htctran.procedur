  FF2= 'MOT' 'T,Z';
  FF3= 'MOT' '  ';
  LICF='MOTS'  FF1    FF2;
  LICJ='MOTS' 'JX_R' 'JY_Z';
'SINON' ;
 'SI' (('EGA' ('VALE' 'MODE') 'PLANDEFO') 'OU'
       ('EGA' ('VALE' 'MODE') 'PLANCONT'));
   FF1= 'MOT' 'T,X';
   FF2= 'MOT' 'T,Y';
   FF3= 'MOT' '  ' ;
   LICF='MOTS'  FF1    FF2;
   LICJ='MOTS' 'JX_R' 'JY_Z';
 'SINON';
  'SI' ('EGA' ('VALE' 'MODE') 'TRID');
    FF1= 'MOT' 'T,X';
    FF2= 'MOT' 'T,Y';
    FF3= 'MOT' 'T,Z';
    LICF='MOTS'  FF1    FF2    FF3;
    LICJ='MOTS' 'JX_R' 'JY_Z' 'JZ_Z';
  'SINON';
   'MESS' '*** ERREUR -  MODELE INCOMPATIBLE ' ;
   'QUITTER' HTCTRAN;
  'FINSI';
 'FINSI';
'FINSI';
*
 LIPT='MOTS' 'T';
 LICP='MOTS' 'P'    'LX'  ;
 LICT='MOTS' 'T'    'LX'  ;
*
*==================================================
*   ANALISYS OF THE DATA SUPPLIED IN INPUT TABLE
*==================================================
*
*PARAMETRI DI CONTROLLO
*
