  DWT0      = WT1 - WI ;
  WP1 HP1   = HTC_WWW   T0      (P0 + 0.00001) TAV1 ;
  DWP0      = (WP1 - WI)/0.00001 ;
  DWD0      = 'MANU' 'CHPO' MAGLIA 1 'SCAL'  0.;
  WD        = 'MANU' 'CHPO' MAGLIA  1 'SCAL' 0.;
  TAV1.'WDM'=  WD;
*
  CHI='CHAN' 'CHAM' MODL1 WI;
  MI ='INTG' MODL1 CHI;
  MT = MI ;
  PSAT = HTC_WTR TAV1 'SATU' T0 ;
*
  TAV1.'TMAX'=T0;
  T0='NOMC' 'T'  T0;
  P0='NOMC' 'T'  P0;
