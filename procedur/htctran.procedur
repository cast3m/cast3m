'FINSI';
'SI' IFP;
 'SI' ('EXIST' TAV1 'FRONTIERES_PRESSION');
   NFRONT= 'DIME' TAV1.'FRONTIERES_PRESSION';
 'SINON';
  'MESS' '*** ERREUR - DANS LES CONDITIONS AUX LIMITES'
         ' EN PRESSION ' ;
 'FINSI';
'FINSI';
