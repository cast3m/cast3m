 'MESS' '-----------------------------------';
 'MESS' ' MASSE D EAU  INITIALE ' MI;
 'MESS' ' H.R.         INITIALE ' ('MAXI' HI);
 'MESS' ' PRE. SAT.    INITIALE ' ('MAXI' PSAT);
*
  PERMAS=0.;
  IPAS=0;
*
  TAV1.'MI'             = MI;
