$$$$ MAILSTRU
* MAILSTRU  PROCEDUR  CHAT      95/12/08    21:22:14     1934
'DEBPROC'  mailstru co*'MAILLAGE'   dd*'FLOTTANT' ;
dden = &dens;
'DENS' dd  ;
ox = 'COOR' 1 co;
oy = 'COOR' 2 co;
xmin = 'MINI' ox; xmax = 'MAXI' ox;
ymin = 'MINI' oy; ymax = 'MAXI' oy;
pinfg = xmin ymin;
pinfd = xmax ymin;
li = pinfg 'DROI' pinfd;
lsu = li 'TRANS' ( 0 (ymax - ymin));
'DENS' dden;
sinc = lsu 'INCLUS'  co 'STRI';
res = lsu elem  appuy largement    sinc;
'FINPROC'   res;
$$$$

