$$$$ @ZACPRO8
* @ZACPRO8  PROCEDUR  CHAT      95/12/13    21:46:52     1945
*------------------------OPERATEUR PROJECTION------------------------*

'DEBP' @ZACPRO8 MM*'MCHAML' CC*'MCHAML' RR*'MCHAML';
AA       = CC - MM;
ABSAA    = 'ABS' AA;
CRAA1    = ABSAA 'MASQUE' 'INFERIEUR' 1.E-10;
CRAA2    = ABSAA 'MASQUE' 'EGSUPE' 1.E-10;
AA       = AA + CRAA1;
BB       = AA * RR;
NAA      = @ZACPRO7 AA;
NAAM1    = NAA**-1;
PRO      = CC - (BB * NAAM1);
PRO      = PRO * CRAA2;
'FINP' PRO;
