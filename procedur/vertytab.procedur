 Mess '                                                         ' ;
 mess ' On attend des éléments de type QUAF 3D'                   ;
 mess ' Le maillage contient des éléments de type ' el            ;
 Mess '*********************************************************' ;
 Mess                                                             ;
 ERREUR 21                                                        ;
 quitter VERTYTAB                                       ;
  Finsi                                                 ;
 FIN BTYEL                                              ;
 FINSI                                                  ;

 FINSI                                                  ;


SI (KAS 'EGA' 2)                                        ;
 TYEL = GEO.'TYEL'                                      ;
 nbe  = dime ltpvf                                      ;

 Si ('EGA' TYEL 'LIN2D')                                ;
