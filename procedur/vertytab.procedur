Si (Exist KAS)                                          ;
 ARGU GEO*'TABLE'  mt*'MAILLAGE'                        ;
 ltpvf = elem mt 'TYPE'                                 ;
 nbe= dime ltpvf                                        ;

 Ellin1D= mots 'SEG2'                                   ;
 Ellin2D= mots 'TRI3' 'QUA4'                            ;
 Ellin3D= mots 'CUB8' 'PRI6' 'TET4' 'PYR5'              ;
 Elqad1D= mots 'SEG3'                                   ;
 Elqad2D= mots 'TRI6' 'QUA8'                            ;
 Elqad3D= mots 'CU20' 'PR15' 'TE10' 'PY13'              ;
 Elqaf1D= mots 'SEG3'                                   ;
 Elqaf2D= mots 'TRI7' 'QUA9'                            ;
 Elqaf3D= mots 'CU27' 'PR21' 'TE15' 'PY19'              ;
 elref=extr ltpvf 1                                     ;

 Si ('EGA' KAS 0)                                       ;

 Si ('EXIST' Ellin2D elref)                             ;
 TYEL='LIN2D'                                           ;
 Finsi                                                  ;
 Si ('EXIST' Ellin3D elref)                             ;
 TYEL='LIN3D'                                           ;
 Finsi                                                  ;

 Si ('EXIST' Elqad2D elref)                             ;
 TYEL='QAD2D'                                           ;
 Finsi                                                  ;
 Si ('EXIST' Elqad3D elref)                             ;
 TYEL='QAD3D'                                           ;
 Finsi                                                  ;

 Si ('EXIST' Elqaf2D elref)                             ;
 TYEL='QAF2D'                                           ;
 Finsi                                                  ;
 Si ('EXIST' Elqaf3D elref)                             ;
 TYEL='QAF3D'                                           ;
 Finsi                                                  ;

 GEO.'TYEL' = TYEL                                      ;
mess                                                    ;
mess ' On vérifie la cohérence des maillages'           ;
mess ' Le premier type d élément du maillage donné pour vtf est ' ELREF;
mess ' On vérifie que les éléments des maillages fournis pour les';
mess ' besoins du calcul (parois, conditions limites etc) sont'   ;
mess ' compatibles avec les éléments géométriques de type ' TYEL  ;
mess ' et ceci indépendamment de la discrétisations choisie.'     ;
mess ' Les points d un arc de cercle suivant qu il est maillé en ';
mess ' LINE ou en QUAD ne sont pas forcément à la même position.' ;
mess                                                    ;

FINSI                                                   ;

SI (KAS '<EG' 1)                                        ;
 TYEL = GEO.'TYEL'                                      ;
 nbe= dime ltpvf                                        ;

 Si ('EGA' TYEL 'LIN2D')                                ;
 REPETER BTYEL nbe                                      ;
  el=extr ltpvf &BTYEL                                  ;
  Si (NON ('EXIST' Ellin2D el))                         ;
 Mess                                                             ;
 Mess '*********************************************************' ;
 Mess ' ERREUR   ERREUR   ERREUR   ERREUR   ERREUR   ERREUR     ' ;
 Mess '                                                         ' ;
 Mess '                                                         ' ;
 mess ' On attend des éléments de type linéaire 2D '              ;
 mess ' Le maillage contient des éléments de type ' el  ;
 Mess '*********************************************************' ;
 Mess                                                             ;
 ERREUR 21                                                        ;
 quitter VERTYTAB                                       ;
  Finsi                                                 ;
 FIN BTYEL                                              ;
 FINSI                                                  ;

