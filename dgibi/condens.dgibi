TAB1 = TABLE ;
TAB1.1 = 'REGU MARQ CROI ' ;
TAB1.2 = 'REGU MARQ PLUS ' ;
TAB1.3 = 'REGU MARQ CARR ' ;
TAB1.4 = 'REGU MARQ TRIA ' ;
