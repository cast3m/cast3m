     TN = kops RV.'INCO'.'TN'  '-' 273.15 ;
  'TRAC'      TN  MFLUID  cnt 'TITR' 'TEMPERATURE DU GAZ (en °C)' ;

    nut = RV.INCO.'NUT' ;
