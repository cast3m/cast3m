   (
   (('EXTRAIRE' GRADRN1 'P1DX' PG) * DXG) '+'
   (('EXTRAIRE' GRADRN1 'P1DY' PG) * DYG) '+'
   (('EXTRAIRE' GRADRN1 'P1DZ' PG) * DZG)
   ) * ('EXTRAIRE' LIMRN1 'P1' PG)) ;
rn2gn = ('EXTRAIRE' RN2 'SCAL' PG) '+' (
