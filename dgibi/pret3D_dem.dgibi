   (
   (('EXTRAIRE' GRADPN1 'P1DX' PD) * DXD) '+'
   (('EXTRAIRE' GRADPN1 'P1DY' PD) * DYD) '+'
   (('EXTRAIRE' GRADPN1 'P1DZ' PD) * DZD)
   ) * ('EXTRAIRE' LIMPN1 'P1' PD)) ;
