ux1dn = ('EXTRAIRE' VN1 'UX  ' PD) '+' (
   (
   (('EXTRAIRE' GRADUX1 'P1DX' PD) * DXD) '+'
   (('EXTRAIRE' GRADUX1 'P1DY' PD) * DYD) '+'
   (('EXTRAIRE' GRADUX1 'P1DZ' PD) * DZD)
