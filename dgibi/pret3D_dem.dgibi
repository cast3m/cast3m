   (('EXTRAIRE' GRADAL1 'P1DX' PG) * DXG) '+'
   (('EXTRAIRE' GRADAL1 'P1DY' PG) * DYG)'+'
   (('EXTRAIRE' GRADAL1 'P1DZ' PG) * DZG)
