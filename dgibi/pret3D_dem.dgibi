'SI' (ERRO > 1.0D-12) ;
   'MESSAGE' 'Deuxieme ordre' ;
   'MESSAGE' 'Les etats droites' ;
   'MESSAGE' 'Erreur trop importante' ;
   'MESSAGE' ('CHAINE' 'erro = ' ERRO) ;
