   (('EXTRAIRE' GRADPN2 'P1DX' PD) * DXD) '+'
   (('EXTRAIRE' GRADPN2 'P1DY' PD) * DYD) '+'
   (('EXTRAIRE' GRADPN2 'P1DZ' PD) * DZD)
