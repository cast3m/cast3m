 DS2S2N = JACNUM $DOMTOT METO SN UN  PCEN6 PCEN9
   'PN' 'PN' DELTA ;
 DS3S3N = JACNUM $DOMTOT METO SN UN  PCEN6 PCEN9
   'GAMN' 'GAMN' DELTA ;

