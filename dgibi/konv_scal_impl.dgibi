 DELTA = 1.0D-3 ;

 DS1S1N = JACNUM $DOMTOT METO SN UN  PCEN9 PCEN6
   'RN' 'RN' DELTA ;
 DS2S2N = JACNUM $DOMTOT METO SN UN  PCEN9 PCEN6
   'PN' 'PN' DELTA ;
 DS3S3N = JACNUM $DOMTOT METO SN UN  PCEN9 PCEN6
   'GAMN' 'GAMN' DELTA ;

