DB1 = 'DBIT' ('MODE' ('CONT' Mm1) 'NAVIER_STOKES' DISCR) an 'IMPR' ;
DB2 = 5.60947571 ;
err1=1.E-8 ;
Si ( (abs (DB1 - DB2)) > err1 ) ; ERROR = ERROR + 1 ; finsi ;

******************** MACRO ******************************
