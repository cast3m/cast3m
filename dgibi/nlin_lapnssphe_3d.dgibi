'OPTION' 'ECHO' 1 ;
************************************************************************
* NOM         : NLIN_LAPNSSPHE_3D
* DESCRIPTION : Vérification de NLIN : on résout un laplacien sur un
*               bout de surface de sphère de rayon R en 3D. La
*               solution exacte est une fonction du type :
*               a * ln (tan (\theta / 2)) + b
*               Il y a une singularité en theta = 0. Si on veut observer
*               l'ordre correct pour theta petit, il faut bien raffiner.
*
*               c'est une primitive de a / sin (\theta).
*               On vérifie les ordres de convergence sur cette solution
*               et sur la surface calculée du bout de sphère.
*
* LANGAGE     : GIBIANE-CAST3M
* AUTEUR      : Stéphane GOUNAND (CEA/DEN/DM2S/SFME/LTMF)
*               mél : gounand@semt2.smts.cea.fr
**********************************************************************
* VERSION    : v1, 23/10/2006, version initiale
* HISTORIQUE : v1, 23/10/2006, création
* HISTORIQUE :
* HISTORIQUE :
************************************************************************
* Prière de PRENDRE LE TEMPS de compléter les commentaires
* en cas de modification de ce sous-programme afin de faciliter
* la maintenance !
************************************************************************
*
*
interact = FAUX ;
graph    = FAUX ;
*interact = VRAI ;
*graph    = VRAI ;
'OPTION' 'DIME' 3 ;
'SI' ('NON' interact) ;
  'OPTION' 'TRAC' 'PS' 'ECHO' 0 ;
'SINON' ;
  'OPTION' 'TRAC' 'X' 'ECHO' 1 ;
'FINSI' ;
*
* Paramètres
*
ldisc = 'MOTS' 'LINE' 'QUAI' ;
ttamin  = 40. ;
ttaminr = ttamin '*' ('/' PI 180.) ;
tordsur = 'TABLE' ;
tordsol = 'TABLE' ;
ldens = 'PROG' 2.D-1 1.D-1 5.D-2 ;
*ldens = 'PROG' 1.D-1 5.D-2 1.D-2 ;
rayo  = '**' 2. 0.5D0 ;
tabev = 'TABLE' ;
tabt  = 'TABLE' ;
itab = 0 ;
ok = VRAI ;
*
* Boucle sur les discrétisations
*
'REPETER' iidisc ('DIME' ldisc) ;
   idisc = &iidisc ;
   disc  = 'EXTRAIRE' ldisc idisc ;
   tordsur . disc = 'TABLE' ;
   tordsol . disc = 'TABLE' ;
   discg = disc ;
   disct = disc ;
   'SI' ('EGA' disc 'LINE') ;
      'OPTION' 'ELEM' 'TRI3' ;
   'SINON' ;
      'OPTION' 'ELEM' 'TRI6' ;
   'FINSI' ;
   lerrsur = 'PROG' ;
   lerrsol = 'PROG' ;
   'REPETER' iidens ('DIME' ldens) ;
      idens = &iidens ;
      vdens = 'EXTRAIRE' ldens idens ;
      'DENS' vdens ;
      tit = 'CHAINE' 'Discretisation :' disc '  Densité = ' vdens ;
      'SAUTER' 1 'LIGNE' ;
      'MESSAGE' tit ;
* Maillage
      phi = 40. ;
      phir = 40. '*' ('/' PI 180.) ;
      p0 = 'POIN' 0. 0. 0. ;
      p1 = 'POIN' 0. 0. ('*' ('COS' ttamin) rayo) ;
      pA = POINTSPH rayo 0. 0. ;
      pB = POINTSPH rayo phi 0. ;
      pC = POINTSPH rayo phi ('-' 90. ttamin) ;
      pD = POINTSPH rayo 0.  ('-' 90. ttamin) ;
      lAB = 'CERCLE' pA p0 pB ;
      lBC = 'CERCLE' pB p0 pC ;
      lCD = 'CERCLE' pC p1 pD ;
      lDA = 'CERCLE' pD p0 pA ;
      cmt = lAB 'ET' lBC 'ET' lCD 'ET' lDA ;
*      cmt = lCD ;
*      xmt ymt zmt = 'COORDONNEE' cmt ;
*      rmt = '**' (('**' xmt 2) '+' ('**' ymt 2) '+' ('**' zmt 2))
*                 0.5D0 ;
*      'LISTE' ('-' ('MAXIMUM' rmt) rayo) ;
*      'LISTE' ('-' ('MINIMUM' rmt) rayo) ;
*      'OPTION' 'DONN' 5 ;
      mt = 'SURFACE' cmt 'SPHERIQUE' p0 ;
*      'OPTION' 'DONN' 5 ;
*      mt = 'PROJETER' mt 'CONI' p0 'SPHE' p0 pA ;
*      lAB = 'PROJETER' lAB 'CONI' p0 'SPHE' p0 pA ;
*      lBC = 'PROJETER' lBC 'CONI' p0 'SPHE' p0 pA ;
*      lCD = 'PROJETER' lCD 'CONI' p0 'SPHE' p0 pA ;
*      lDA = 'PROJETER' lDA 'CONI' p0 'SPHE' p0 pA ;
*      'ELIMINATION' (mt 'ET' lAB 'ET' lBC 'ET' lCD 'ET' lDA)
*                    ('/' vdens 100.) ;
      'SI' graph ;
         tit = 'CHAINE' 'Maillage' ;
         'TRACER'  'CACH' mt 'TITR' tit ;
      'FINSI' ;
      _mt = 'CHANGER' mt 'QUAF' ;
* 1 : calcul de la surface du bout de sphère
      ch1 = 'MANUEL' 'CHPO' mt 1 'T' 1. ;
      gmas = GMASS _mt discg 'T' disct 'Q' disct 1. ;
      surana = phir '*' rayo '*' rayo '*' ('COS' ttamin) ;
      sur = xtmx ch1 gmas ;
      'MESSAGE' ('CHAINE' 'Surface calculee =' sur) ;
      'MESSAGE' ('CHAINE' 'Surface attendue =' surana) ;
      errsur = 'ABS' ('-' sur surana) ;
      'MESSAGE' ('CHAINE' 'Erreur sur la surface =' errsur) ;
      lerrsur = 'ET' lerrsur ('PROG' errsur) ;
* 2 : calcul d'une solution analytique
      xmt ymt zmt = 'COORDONNEE' mt ;
      rmt = '**' ('+' ('**' xmt 2) ('**' ymt 2)) 0.5D0  ;
      tta = 'ATG' rmt zmt ;
      ttamsur2 = '/' ttamin 2. ;
      coef = 'LOG' ('/' ('SIN' ttamsur2) ('COS' ttamsur2)) ;
      ttasur2 = '/' tta 2.D0 ;
      solana = '/' ('LOG' ('/' ('SIN' ttasur2) ('COS' ttasur2))) coef ;
      solana = 'NOMC' 'T' solana ;
      mat = GLAPN _mt discg 'T' disct 'Q' disct 1. ;
      mblo = 'BLOQUE' 'T' (lAB 'ET' lCD) ;
      cblo = '+' ('MANUEL' 'CHPO'  lAB 1 'T' 0.)
                 ('MANUEL' 'CHPO'  lCD 1 'T' 1.) ;
*      mblo = 'BLOQUE' 'T' cmt ;
*      cblo = 'REDU' solana cmt ;
      fblo = 'DEPIMPOSE' mblo cblo ;
*
      mtot = mat 'ET' mblo ;
      ftot = fblo ;
      solu = 'RESOUD' mtot ftot ;
      sol  = 'EXCO' 'T' solu 'T' ;
      matm = GMASS _mt discg 'T' disct 'Q' disct 1. ;
      errsol = '**' (xtmx ('-' sol solana) matm) 0.5D0 ;
      'MESSAGE' ('CHAINE' 'Erreur sur la sol. analytique =' errsol) ;
      'SI' graph ;
         tit = 'CHAINE' 'Sol Approchée' ;
         'TRACER'  sol mt 'TITR' tit ;
         tit = 'CHAINE' 'Sol Approchée moins Analytique' ;
         'TRACER'  ('-' sol solana) mt 'TITR' tit ;
      'FINSI' ;
*      'OPTION' 'DONN' 5 ;
      lerrsol = 'ET' lerrsol ('PROG' errsol) ;
   'FIN' iidens ;
* Calcul des ordres
   lh lerrsur lerrsol = LOG10 ldens lerrsur lerrsol ;
   evsur = 'EVOL' 'MANU' lh lerrsur ;
   cpl dummy = @POMI evsur 1 'IDEM' ;
   ord = cpl . 1 ;
   tordsur . disc = ord ;
   itab = '+' itab 1 ;
   tabev . itab = evsur ;
   tabt  . itab = 'CHAINE' 'sur;' disc ';ord=' (formar ord 1) ;
*
   evsol = 'EVOL' 'MANU' lh lerrsol ;
   cpl dummy = @POMI evsol 1 'IDEM' ;
   ord = cpl . 1 ;
   tordsol . disc = ord ;
   itab = '+' itab 1 ;
   tabev . itab = evsol ;
   tabt  . itab = 'CHAINE' 'sol;' disc ';ord=' (formar ord 1) ;
'FIN' iidisc ;
* Tracés
'SI' graph ;
   evt = @STBL tabev ;
   tit = 'CHAINE' 'Ordre de convergence sur : surface, solution du '
                  'laplacien'  ;
   tix = 'CHAINE' 'Log10 h' ;
   tiy = 'CHAINE' 'Log10 err' ;
   DESSEVOL evt tabt tit tix tiy ;
'FINSI' ;
