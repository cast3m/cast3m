*  OBJET TABTR
*     ELEM(I,J)       CONTENUE DE LA CASE COLONNE M, LIGNE N
*     YTYPE(I)        TYPE DES ELEMENTS DE LA COLONNE I
*     ILARG(I)	      LARGEUR DE LA COLONNE I
*     ZVSEP(I,J)      SEPARATION VERTICAL A GAUCHE DE LA CASE (I,J)
*     ZHSEP(I,J)      SEPARATION HORIZONTALE AU DESSUS
*			DE LA CASE (I,J)
*     ZGVSEP(I,J)    SEPARATION VERTICAL EN GRAS A GAUCHE
*			DE LA CASE (I,J)
*     ZGHSEP(I,J)    SEPARATION HORIZONTALE EN GRAS AU DESSUS
*			DE LA CASE (I,J)
*     TITGEN        TITRE DU TABLEAU
*     SSTITR        SOUS TITRE POUR LE TABLEAU
*     ZCADRE	      ENCADRER LE TITRE
*     TITCOL(I)     TITRE DE LA COLONNE I
*     IHTCOL          HAUTEUR DES TITRES DE COLONNE EN LIGNES
*     ZDATE           METTRE LA DATE
*     ZPAGE           METTRE LES NUMEROS DE PAGE
*     ZCTRER	      CENTRER LES TABLEAUX
*     ZAULIG     METTRE AUTOMATIQUEMENT LES ENCADREMENTS
*     ZLOGO	      METTRE LE LOGO
*     XLPOS,YLPOS     POSITION DU LOGO
*     TLOGO	      TAILLE DU LOGO
*     CSGX(I,J)	      NUMERO DE COL DU COIN SUP GAUCHE DE LA PAGE I,J
*     CSGY(I,J)	      NUMERO DE LIG DU COIN SUP GAUCHE DE LA PAGE I,J
*     CIDX(I,J)	      NUMERO DE COL DU COIN INF DROIT DE LA PAGE I,J
*     CIDY(I,J)	      NUMERO DE LIG DU COIN INF DROIT DE LA PAGE I,J
*     IHDEC(I,J)      DECALAGE HORIZONTAL POUR CENTRER LA PAGE I,J
*     IVDEC(I,J)      DECALAGE VERTICAL POUR CENTRER LA PAGE I,J
*     PX              NOMBRE DE PAGES EN X
*     PY              NOMBRE DE PAGES EN Y
*     ITEXC	      COULEUR DU TEXTE
*     ILIGC	      COULEUR DES LIGNES
*     IINDC	      COULEUR DES INDICATIONS
*     ITITC	      COULEUR DES TITRES
*     ILOGC	      COULEUR DU LOGO
*
* FONCTIONS
*  F_ATOI		CONVERTIE UNE CHAINE EN ENTIER
*  F_ATOL		CONVERTIE UNE CHAINE EN REAL*8
*  F_LTOA		CONVERTIE UN REEL EN CHAINE
*  F_ITOA		CONVERTIE UN ENTIER EN CHAINE
*  ZEGALE	TEST SI 2 REAL*8 SONT EGAUX A EPSILON PRES
*  ZTEST	FONCTION DE TEST AVEC CLE POUR LES TRIS
*
        SEGMENT TABTR
           CHARACTER*30   ELEM   (ITABX,  ITABY  )
	   CHARACTER*8	  YTYPE	 (ITABX)
	   LOGICAL        ZVSEP  (ITABX+1,ITABY+1)
	   LOGICAL        ZHSEP  (ITABX+1,ITABY+1)
	   LOGICAL        ZGVSEP(ITABX+1,ITABY+1)
	   LOGICAL        ZGHSEP(ITABX+1,ITABY+1)
           CHARACTER*128  TITGEN
	   LOGICAL 	  ZCADRE
           CHARACTER*128  SSTITR
	   CHARACTER*72   TITCOL(ITABX)
	   INTEGER	  IHTCOL
	   LOGICAL        ZDATE
	   LOGICAL	  ZPAGE
	   LOGICAL	  ZCTRER
	   LOGICAL	  ZAULIG
	   LOGICAL	  ZLOGO
	   REAL*4	  XLPOS,YLPOS
	   REAL*4	  TLOGO
	   INTEGER	  CSGX   (PAGESX,PAGESY)
	   INTEGER	  CSGY   (PAGESX,PAGESY)
	   INTEGER	  CIDX   (PAGESX,PAGESY)
	   INTEGER	  CIDY   (PAGESX,PAGESY)
	   INTEGER	  IHDEC  (PAGESX,PAGESY)
	   INTEGER	  IVDEC  (PAGESX,PAGESY)
	   INTEGER	  PX,PY
	   INTEGER	  ILIGC,ITEXC,ICOLC,ITITC,ILOGC
	   LOGICAL        ZINDIC
        ENDSEGMENT
*
*
	INTEGER		F_ATOI
	REAL*8		F_ATOL
	CHARACTER*10	F_ITOA
	CHARACTER*10	F_LTOA
	LOGICAL		TBZGAL
	LOGICAL		TBZTES
*
