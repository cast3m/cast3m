                                            CHPO4  CHPO5  CHPO6
                                            CHPO7  CHPO8  CHPO9
                                           (CHPO10 CHPO11 CHPO12)
                                           (CHPO13 CHPO14 CHPO15)
                                            CHPO16  FLOT1 ;

